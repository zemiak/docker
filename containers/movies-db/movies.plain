INSERT INTO movie VALUES (868, 'Alien/Alien.m4v', 'Alien', 4, 4, 'en', 'en', 'en', NULL, 'http://www.imdb.com/title/tt0090605/', 'Alien.jpg', 1, 'Directed by James Cameron. With Sigourney Weaver, Michael Biehn, Carrie Henn, Paul Reiser. The planet from Alien has been colonized, but contact is lost. This time, the rescue team has impressive firepower, but will it be enough?');
