INSERT INTO movie VALUES (1456, 'Lord of the Rings/LOTR-1-FOTR-EXT-EN-A.m4v', 'Fellowship of The Ring, ext, 1', 4, 2, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0120737/', 'LOTR-1-FOTR-EXT-EN-A.jpg', 7, 'Directed by Peter Jackson. With Elijah Wood, Ian McKellen, Orlando Bloom, Sean Bean. An innocent hobbit of The Shire journeys with eight companions to the fires of Mount Doom to destroy the One Ring and the dark lord Sauron forever.');
