INSERT INTO movie VALUES (1121, 'Lord of the Rings/1-FOTR-STD-A.m4v', 'Spolecenstvo prstenu, 1', 4, 2, 'cz', '  ', 'en', 'Fellowship of The Ring, part 1', 'http://www.csfd.cz/film/4711-pan-prstenu-spolecenstvo-prstenu/', '1-FOTR-STD-A.jpg', 13, 'Hlavním hrdinou je hobit Frodo Pytlík, který se ve své vlastní domovině zvané Středozemě dostal do samotného centra bitvy mytických proporcí. Bude muset během svého putování prokázat, že moc přátelství a individuální odvahy dokáže úspěšně čelit i těm nejničivějším silám temnoty. Frodova dobrodružství odstartuje dar jeho příbuzného Bilba. Od něj získá magický prsten, který Froda postaví do soboje se silami zla reprezentovanými krutovládcem Sauronem. Ten touží za každou cenu získat prsten, protože je pro něj zdrojem neomezené moci, jedině s jeho pomocí se mu může podařit zotročit lid Středozemě a stát se jeho neomezeným pánem. Ve snaze čelit Sauronovým temným plánům, se musí Frodo spojit se několika dalšími bytostmi. Vytvoří s nimi takzvané Společenstvo složené z jeho přátel, čaroděje, elfa, trpaslíka a dalších pomocníků, se kterými se vydá na cestu na jejímž konci doufají, že se jim podaří prsten jednou provždy zničit. Jejich cesta však prokáže, že nejenom magické prsteny, ale také sounáležitost všech živých bytostí mohou dokázat netušené věci. Musí však čelit nejen nebezpečím, která na ně číhají na územích ovládaných Sauronem, ale i rozkladné moci samotného prstenu.');
INSERT INTO movie VALUES (1122, 'Lord of the Rings/1-FOTR-STD-B.m4v', 'Spolecenstvo prstenu, 2', 4, 2, 'cz', '  ', 'en', 'Fellowship of The Ring, part 2', 'http://www.csfd.cz/film/4711-pan-prstenu-spolecenstvo-prstenu/', '1-FOTR-STD-B.jpg', 14, 'Hlavním hrdinou je hobit Frodo Pytlík, který se ve své vlastní domovině zvané Středozemě dostal do samotného centra bitvy mytických proporcí. Bude muset během svého putování prokázat, že moc přátelství a individuální odvahy dokáže úspěšně čelit i těm nejničivějším silám temnoty. Frodova dobrodružství odstartuje dar jeho příbuzného Bilba. Od něj získá magický prsten, který Froda postaví do soboje se silami zla reprezentovanými krutovládcem Sauronem. Ten touží za každou cenu získat prsten, protože je pro něj zdrojem neomezené moci, jedině s jeho pomocí se mu může podařit zotročit lid Středozemě a stát se jeho neomezeným pánem. Ve snaze čelit Sauronovým temným plánům, se musí Frodo spojit se několika dalšími bytostmi. Vytvoří s nimi takzvané Společenstvo složené z jeho přátel, čaroděje, elfa, trpaslíka a dalších pomocníků, se kterými se vydá na cestu na jejímž konci doufají, že se jim podaří prsten jednou provždy zničit. Jejich cesta však prokáže, že nejenom magické prsteny, ale také sounáležitost všech živých bytostí mohou dokázat netušené věci. Musí však čelit nejen nebezpečím, která na ně číhají na územích ovládaných Sauronem, ale i rozkladné moci samotného prstenu.');
INSERT INTO movie VALUES (1137, 'MASH/MASH 011.mp4', 'Banány, krekry a oříšky', 11, 1, 'cz', '  ', 'en', 'Bananas, Crackers and Nuts', 'http://www.csfd.cz/film/68850-mash/', 'MASH 011.jpg', 11, 'Hawkeymu zruší dovolenou v Tokiu, proto se snaží předstírat šílenství a způsobí mnoho problémů psychiatrovi, který ho na popud Franka přijede sledovat. Hawkeye se navíc Frankovi snaží namluvit, že se o něj zajímá Margret.');
