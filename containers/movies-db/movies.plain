INSERT INTO movie VALUES (1541, '201208/Finley 6-6 Zmena pocasia.mp4', 'Zmena pocasia', 3, 30, 'sk', NULL, 'en', NULL, NULL, 'Finley 6-6 Zmena pocasia.jpg', 66, NULL);
INSERT INTO movie VALUES (1123, 'Lord of the Rings/2-TTT-STD-A.m4v', 'Dve veze, 1', 4, 2, 'cz', '  ', 'en', 'The Two Towers, part 1', 'http://www.csfd.cz/film/4713-pan-prstenu-dve-veze/', '2-TTT-STD-A.jpg', 15, 'Druhý díl filmové ságy o nebezpečném putování jehož cílem má být zničení prokletého prstenu moci začíná tam kde první skončil. Opět je hlavním hrdinou hobit Frodo. Ten se získal od svého příbuzného Bilba magický prsten po kterém touží temný Sauron a tak se dostal do samotného centra souboje mezi silami světla a temnot. Frodo jako vůdce společenství složeného z jeho přátel, kouzelníka, elfa, trpaslíka a dalších pomocníků pokračuje na cestě na vrchol sopky, aby zde prsten jednou provždy zničil. První díl byl nominován na Zlatý glóbus a získal cenu AFI. Díky zájmu diváků překročil zisk 350 milionů dolarů a především přesvědčil příznivce knihy, že tento dnes již klasický příběh lze úspěšně zfilmovat. V nezměněné sestavě vám teď tvůrci předkládají druhý díl fantaskního putování.');
INSERT INTO movie VALUES (1124, 'Lord of the Rings/2-TTT-STD-B.m4v', 'Dve veze, 2', 4, 2, 'cz', '  ', 'en', 'The Two Towers, part 2', 'http://www.csfd.cz/film/4713-pan-prstenu-dve-veze/', '2-TTT-STD-B.jpg', 16, 'Druhý díl filmové ságy o nebezpečném putování jehož cílem má být zničení prokletého prstenu moci začíná tam kde první skončil. Opět je hlavním hrdinou hobit Frodo. Ten se získal od svého příbuzného Bilba magický prsten po kterém touží temný Sauron a tak se dostal do samotného centra souboje mezi silami světla a temnot. Frodo jako vůdce společenství složeného z jeho přátel, kouzelníka, elfa, trpaslíka a dalších pomocníků pokračuje na cestě na vrchol sopky, aby zde prsten jednou provždy zničil. První díl byl nominován na Zlatý glóbus a získal cenu AFI. Díky zájmu diváků překročil zisk 350 milionů dolarů a především přesvědčil příznivce knihy, že tento dnes již klasický příběh lze úspěšně zfilmovat. V nezměněné sestavě vám teď tvůrci předkládají druhý díl fantaskního putování.');
INSERT INTO movie VALUES (1125, 'Lord of the Rings/3-ROTK-STD-A.m4v', 'Navrat krale, 1', 4, 2, 'cz', '  ', 'en', 'Return of The King, part 1', 'http://www.csfd.cz/film/4712-pan-prstenu-navrat-krale/', '3-ROTK-STD-A.jpg', 17, 'Nadchází čas rozhodující bitvy o přežití Středozemě. Putování jednotlivých členů Společenstva prstenu se dostává do poslední a rozhodující fáze. Čaroděj Gandalf, elf Legolas a trpaslík Gimli spěchají s dědicem trůnu Aragornem na pomoc zemi Gondor, která odolává ohromnému Sauronovu vojsku. Hobiti Frodo a Sam se v doprovodu Gluma snaží dostat hluboko do země Mordor, kde musí v ohních Hory osudu zničit magický Prsten moci. Jedině tak bude síla mocného pána temnot Saurona zlomena. Podaří se jim naplnit poslání Společenstva a zachránit Středozem? A za jakou cenu?');
INSERT INTO movie VALUES (1126, 'Lord of the Rings/3-ROTK-STD-B.m4v', 'Navrat krale, 2', 4, 2, 'cz', '  ', 'en', 'Return of The King, part 2', 'http://www.csfd.cz/film/4712-pan-prstenu-navrat-krale/', '3-ROTK-STD-B.jpg', 18, 'Nadchází čas rozhodující bitvy o přežití Středozemě. Putování jednotlivých členů Společenstva prstenu se dostává do poslední a rozhodující fáze. Čaroděj Gandalf, elf Legolas a trpaslík Gimli spěchají s dědicem trůnu Aragornem na pomoc zemi Gondor, která odolává ohromnému Sauronovu vojsku. Hobiti Frodo a Sam se v doprovodu Gluma snaží dostat hluboko do země Mordor, kde musí v ohních Hory osudu zničit magický Prsten moci. Jedině tak bude síla mocného pána temnot Saurona zlomena. Podaří se jim naplnit poslání Společenstva a zachránit Středozem? A za jakou cenu?');
INSERT INTO movie VALUES (1127, 'MASH/MASH 001.mp4', 'M*A*S*H - Úvodní epizoda', 11, 1, 'cz', '  ', 'en', 'The First Part', 'http://www.csfd.cz/film/68850-mash/', 'MASH 001.jpg', 1, 'Korejský chlapec Ho John je přijat ke studiu na škole v Maine, kde vystudoval Hawkeye. Aby sehnali peníze na jeho cestu, Hawkeye s Traperem uspořádají přes odpor Burnse a Štabajzny loterii o víkend s nejhezčí sestrou v Tokiu. Vítězem se stane ke všeobecnému překvapení otec Mulcahy.');
INSERT INTO movie VALUES (1128, 'MASH/MASH 002.mp4', 'Henry, prosím, vrať se domu', 11, 1, 'cz', '  ', 'en', 'Henry, Please Come Home', 'http://www.csfd.cz/film/68850-mash/', 'MASH 002.jpg', 2, 'Díky úspěchům jednotky je velitel Henry za odměnu převelen generálem Hammondem na lepší místo do Tokia. Velení se ujme Frank Burns. Hawkeye a Traper dělají všechno pro to, aby přiměli Henryho k návratu. Vydají se do Tokia a po dohodě s Radarem předstírají, že je mu zle a Henry proto musí přijet.');
INSERT INTO movie VALUES (1129, 'MASH/MASH 003.mp4', 'Na trh', 11, 1, 'cz', '  ', 'en', 'To market, to market', 'http://www.csfd.cz/film/68850-mash/', 'MASH 003.jpg', 3, 'Obchodníci na černém trhu ukradnou z jednotky zásoby hydrocortisonu. Hawkeye a Traper proto kontaktují místního obchodníka, aby nějaký získali. Výměnou za hydrocortison nabídnou Henryho starožitný dubový stůl, který nakonec pomocí vrtulníku ukradnou.');
INSERT INTO movie VALUES (1130, 'MASH/MASH 004.mp4', 'Bakteriologická válka', 11, 1, 'cz', '  ', 'en', 'Germ Warfare', 'http://www.csfd.cz/film/68850-mash/', 'MASH 004.jpg', 4, 'Hawkeye uloží raněného severokorejského vojáka do Bažiny, aby nebyl odvezen dřív, než ho doléčí. Potřebuje krev a tak si hraje na Drákulu a potají odebere Frankovu krev. Když získá podezření, že voják má žloutenku, snaží se Franka společně s Radarem a Traperem dostat stranou od všech, než přijdou výsledky z laboratoře.');
INSERT INTO movie VALUES (1131, 'MASH/MASH 005.mp4', 'Služka', 11, 1, 'cz', '  ', 'en', 'The Moose', 'http://www.csfd.cz/film/68850-mash/', 'MASH 005.jpg', 5, 'Hawkeye vyhraje v pokeru korejskou dívku, která sloužila seržantovi Bakerovi. Dívka ale dlouho nedokáže pochopit, že jí nabízí svobodu. Hawkeye ji musí naučit, jak se vypořádat s novou situací.');
INSERT INTO movie VALUES (1132, 'MASH/MASH 006.mp4', 'Nenávidím tajemství', 11, 1, 'cz', '  ', 'en', 'I Hate A Mystery', 'http://www.csfd.cz/film/68850-mash/', 'MASH 006.jpg', 6, 'Série krádeží v táboře udělá z Hawkeyeho detektiva, tím se ovšem stane hlavně z nouze, protože je hlavním podezřelým. Nakonec odhalí viníka - je jím Ho John, který stále neodjel do USA a potřebuje peníze na podplacení stráží, aby se dostal ke své rodině ze severu.');
INSERT INTO movie VALUES (1133, 'MASH/MASH 007.mp4', 'Šéfchirurg kdo?', 11, 1, 'cz', '  ', 'en', 'Chief Surgeon Who?', 'http://www.csfd.cz/film/68850-mash/', 'MASH 007.jpg', 7, 'Frank a Štabajzna jsou pobouřeni, když jmenuje Henry Hawkeyeho šéfchirurgem místo Franka. Zavolají generála Barkera, který přijede do tábora na inspekci. Potkává tu Klingera (jeho první účinkování v seriálu), prohlásí ho za blázna a přesvědčí se, že Hawkeye je geniální chirurg.');
INSERT INTO movie VALUES (1134, 'MASH/MASH 008.mp4', 'Requiem pro lehkou váhu', 11, 1, 'cz', '  ', 'en', 'Requiem for a Lightweight', 'http://www.csfd.cz/film/68850-mash/', 'MASH 008.jpg', 8, 'Traper se pod vedení kouče Hawkeyeho zúčastní mezijednotkového turnaje v boxu, aby zachránil krásnou sestru, která má být přeložena na žádost Štabajzny do jiné jednotky.');
INSERT INTO movie VALUES (1135, 'MASH/MASH 009.mp4', 'Kovboj', 11, 1, 'cz', '  ', 'en', 'Cowboy', 'http://www.csfd.cz/film/68850-mash/', 'MASH 009.jpg', 9, 'Henry odmítne poslat domů postřeleného pilota vrtulníku, který má problémy s manželkou. Pilot nevidí jinou alternativu než zabít Henryho.');
INSERT INTO movie VALUES (1136, 'MASH/MASH 010.mp4', 'Yankee Doodle Doktor', 11, 1, 'cz', '  ', 'en', 'Yankee Doodle Doctor', 'http://www.csfd.cz/film/68850-mash/', 'MASH 010.jpg', 10, 'Do tábora přijíždějí filmaři, aby na přání armádního generála Claytona natočili o jednotce 4077 dokument. Filmování se ujímá Hawkeye, který jej pojme jako pohádku. Při promítání výsledku vyjde najevo, že natočil vlastně parodii, ovšem s vážným koncem. Žádná válka totiž není film.');
INSERT INTO movie VALUES (1138, 'MASH/MASH 012.mp4', 'Edwina', 11, 1, 'cz', '  ', 'en', 'Edwina', 'http://www.csfd.cz/film/68850-mash/', 'MASH 012.jpg', 12, 'Všechny sestry z jednotky se rozhodnou vyhýbat se styky s muži, dokud někdo nevyřeší trápení jejich kolegyně Edwiny, kterou ještě nikdo nepozval na rande. Losem je vybrán Hawkeye a i když jí nakonec pomůže, stojí ho to mnoho útrap.');
INSERT INTO movie VALUES (1139, 'MASH/MASH 013.mp4', 'Drahý táto', 11, 1, 'cz', '  ', 'en', 'Dear Dad', 'http://www.csfd.cz/film/68850-mash/', 'MASH 013.jpg', 13, 'Hawkeye píše dopis svému otci, a humorně v něm líčí situaci ve 4077. Kupříkladu popisuje historku, jak otec Mulcahy rozmluvil Klingerovi úmysl se zabít pomocí ostrého granátu.Ve finále Hawkeye letí v kostýmu Santa Klause na frontu, aby zachránil život zraněnému vojákovi.');
INSERT INTO movie VALUES (1140, 'MASH/MASH 014.mp4', 'Love Story', 11, 1, 'cz', '  ', 'en', 'Love Story', 'http://www.csfd.cz/film/68850-mash/', 'MASH 014.jpg', 14, 'Radar dostane svatební oznámení od své bývalé snoubenky. Hawkeye a Traper se ho snaží rozveselit a seznámí ho s novou sestrou, která miluje literaturu klasickou a hudbu. Margaret se snaží vznikajícímu vztahu zabránit. Radarovo "Ahhhh, Bach!" a "To je opravdu zajímavé" nakonec slaví úspěch.');
INSERT INTO movie VALUES (1141, 'MASH/MASH 015.mp4', 'Tuttle', 11, 1, 'cz', '  ', 'en', 'Tuttle', 'http://www.csfd.cz/film/68850-mash/', 'MASH 015.jpg', 15, 'Hawkeye a Trapper s Radarovou pomocí vytvoří fiktivního kapitána Tuttla, aby z jeho platu mohl byt dotován blízký sirotčinec. Záležitost se jim ale vymkne z rukou. Kapitán Tuttle má být z rozhodnutí generála Claytona dokonce vyznamenán a Hawkeye si musí vymyslet historku o tom, jak kapitán zahynul, když vyskočil z vrtulníku bez padáku.');
INSERT INTO movie VALUES (1142, 'MASH/MASH 016.mp4', 'Rekordman', 11, 1, 'cz', '  ', 'en', 'The Ringbanger', 'http://www.csfd.cz/film/68850-mash/', 'MASH 016.jpg', 16, 'Hawkeye ošetřuje lehké zranění plukovníkovi (Leslie Nielsen), o kterém se dozví, že je rekordman v počtu padlých vojáků pod jeho velením. Frank a Margaret jej obdivují. Hawkeye, Trapper a Radar opijí Henryho a pak zajistí, aby byl plukovník poslán domů, než zase někoho zabije.');
INSERT INTO movie VALUES (1143, 'MASH/MASH 017.mp4', 'Drahý otče...Znovu', 11, 1, 'cz', '  ', 'en', 'Dear Dead, Again', 'http://www.csfd.cz/film/68850-mash/', 'MASH 017.jpg', 17, 'Hawkeye opět píše svému otci o životě v jednotce. Zmiňuje se o ochlazeni vztahu Franka a Štabajzny, o tom, jak se vsadil, že přijde nahý na oběd a jak Radar skládal korespondenční zkoušku na střední školu.');
INSERT INTO movie VALUES (1144, 'MASH/MASH 018.mp4', 'Někdy kulku uslyšíš', 11, 1, 'cz', '  ', 'en', 'Sometimes You Hear the Bullet', 'http://www.csfd.cz/film/68850-mash/', 'MASH 018.jpg', 18, 'Frank si při dovádění s Margaret vyhodí plotýnku a uvažuje o tom, že za toto "technické" zranění ve frontové linii může dostat Pupurové srdce. Mezitím se objeví Hawkeyeho starý přítel, který píše knihu o válce. Později se setkávají znovu - přítel je těžce raněn a umírá. Jeho smrt přinutí Hawkeyeho poslat domu nezletilého vojáka.');
INSERT INTO movie VALUES (1145, 'MASH/MASH 019.mp4', 'Spodky', 11, 1, 'cz', '  ', 'en', 'The Long-John Flap', 'http://www.csfd.cz/film/68850-mash/', 'MASH 019.jpg', 19, 'Je zima a uhodily tuhé mrazy. Hawkeye dostane z domova teplé bavlněné spodky, které mu mnozí závidějí. Když však vidí, jak Traper trpí zimou, daruje mu tyto spodky, které se potom ve formě daru dostanou do rukou mnoha obyvatel tábora, než se vrátí zpět k Hawkeymu.');
INSERT INTO movie VALUES (1146, 'MASH/MASH 020.mp4', 'Major Fred C. Dobbs', 11, 1, 'cz', '  ', 'en', 'Major Fred C. Dobbs', 'http://www.csfd.cz/film/68850-mash/', 'MASH 020.jpg', 20, 'Frank už má všeho dost a po dohodě s Margaret požádá o transfer. Margaret se chce také nechat přeložit, když náhle přijde zpráva, že Hawkeye a Trapper objevili v okolí tábora zlato. To rozhodne - Frank a Šťabajzna zůstávají a stávají se z nich prospektoři. Frank opravdu najde mnoho "zlatých" předmětů, dokonce i zlatý džíp.');
INSERT INTO movie VALUES (1147, 'MASH/MASH 021.mp4', 'Nerozhodně', 11, 1, 'cz', '  ', 'en', 'Sticky Vicket', 'http://www.csfd.cz/film/68850-mash/', 'MASH 021.jpg', 21, 'Hawkeye a Frank se hádají o Frankových chirurgických schopnostech (lépe řečeno neschopnostech). Jeden z Hawkeyho pacientů má potíže a Hawkeye začne pochybovat o svých vlastních schopnostech. Nakonec se ukáže, že chybu udělal Frank.');
INSERT INTO movie VALUES (1148, 'MASH/MASH 022.mp4', 'Armáda versus Námořnictvo', 11, 1, 'cz', '  ', 'en', 'The Army - Navy Game', 'http://www.csfd.cz/film/68850-mash/', 'MASH 022.jpg', 22, 'V průběhu tradičního zápasu armáda versus námořnictvo v americkém fotbalu, který všichni sledují v rádiu, je tábor ostřelován a jedna nevybuchlá bomba zůstane ležet uprostřed tábora. Když se ji Hawkeye a Trapper pokoušejí zneškodnit, vybuchne a vyletí z ní americké propagační letáky.');
INSERT INTO movie VALUES (1149, 'MASH/MASH 023.mp4', 'Příměří', 11, 1, 'cz', '  ', 'en', 'Cease - Fire', 'http://www.csfd.cz/film/68850-mash/', 'MASH 023.jpg', 23, 'Do tábora se donese zpráva od generála Claytona, že se chystá podpis příměří. Vypuknou oslavy, Hawkeye je nucen říkat svým avantýrám, že je ženatý, odpustí pokerové dluhy, jen Traper zprávě nevěří. Naděje v M*A*S*H pohasnou, když dorazí opět noví zranění.');
INSERT INTO movie VALUES (1150, 'MASH/MASH 024.mp4', 'Showtime', 11, 1, 'cz', '  ', 'en', 'Showtime', 'http://www.csfd.cz/film/68850-mash/', 'MASH 024.jpg', 24, 'Kapitán Kaplan je poslán domů, ale sužuje jej utkvělá představa, že se mu na cestě domů něco stane. Rozhodne se jet na letiště sám džípem, má nehodu a skončí na operačním sále. Do tábora přijíždí skupina bavičů Jackieho Flashe a na pár dní se promíchá s osazenstvem.');
INSERT INTO movie VALUES (1151, 'MASH/MASH 025.mp4', 'Rozděleni zůstaneme', 11, 1, 'cz', '  ', 'en', 'Divided We Stand', 'http://www.csfd.cz/film/68850-mash/', 'MASH 025.jpg', 25, 'Generál Clayton pošle psychiatra kpt. Hildebranda na prohlídku 4077. Henry žádá osazenstvo, aby se chovali co nejlépe, nebo budou rozděleni. Marně. Po návštěvě dr. Hildebrant do zprávy napíše, že viděl učebnicové příklady neuróz, psychóz, voyerismu, fetišismu a mnoha dalších ismů. Bylo by proto šílené tyto lidi od sebe oddělit a jejich šílenství ještě posílit.');
INSERT INTO movie VALUES (1152, 'MASH/MASH 026.mp4', 'Radarova zpráva', 11, 1, 'cz', '  ', 'en', 'Radar´s Report', 'http://www.csfd.cz/film/68850-mash/', 'MASH 026.jpg', 26, 'Radar píše týdenní situační zprávu - Hawkeye operuje zraněného vězně, který se zmocní skalpelu a ohrožuje ostatní, pak se snaží flirtovat s novou sestrou, Frank má konflikt s Klingerem, jehož dívka doma v USA se chce vdát, a Henry kvůli tomu povolává psychiatra, majora Freedmana.');
INSERT INTO movie VALUES (1153, 'MASH/MASH 027.mp4', 'Bombarďák Charlie', 11, 1, 'cz', '  ', 'en', '5 O´Clock Charlie', 'http://www.csfd.cz/film/68850-mash/', 'MASH 027.jpg', 27, 'Nešikovnost severokorejského pilota, který se snaží vždy v 17.00 bombardovat blízký sklad střeliva, se stane zábavou pro celý tábor. Frank se snaží prosadit jeho sestřelení, ale dělo dostane k dispozici, až když Charlie trefí džíp generála Claytona. Při výstřelu Frank Charlieho mine a zničí inkriminovaný sklad střeliva.');
INSERT INTO movie VALUES (1154, 'MASH/MASH 028.mp4', 'Pro dobro kolektivu', 11, 1, 'cz', '  ', 'en', 'For the Good of the Outfit', 'http://www.csfd.cz/film/68850-mash/', 'MASH 028.jpg', 28, 'Hawkeye a Trapper upozorní na vybombardování korejské vesnice Tai Dong Američany. Než armáda přijme svou odpovědnost, plánují přestavbu vesnice. Armáda se ale pokouší svést odpovědnost na nepřítele.');
INSERT INTO movie VALUES (1542, '201208/Finley 6-7 Chytte zlodeja.mp4', 'Chytte zlodeja', 3, 30, 'sk', NULL, 'en', NULL, NULL, 'Finley 6-7 Chytte zlodeja.jpg', 67, NULL);
INSERT INTO movie VALUES (1155, 'MASH/MASH 029.mp4', 'Dr. Pierce a pan Hyde', 11, 1, 'cz', '  ', 'en', 'Dr. Pierce a Mr. Hyde', 'http://www.csfd.cz/film/68850-mash/', 'MASH 029.jpg', 29, 'Hawkeye je 3 dny nepřetržitě ve službě a rozhodne se poslat dopis prezidentovi Trumanovi. Pak dostane další nápad a odešle do severní Koreje latrínu s nabídkou k příměří. Traperovi se nakonec podaří jej uklidnit.');
INSERT INTO movie VALUES (1156, 'MASH/MASH 030.mp4', 'M.D.O. - Místní domorodá osoba', 11, 1, 'cz', '  ', 'en', 'L.I.P. - Local Indigenous Personnel', 'http://www.csfd.cz/film/68850-mash/', 'MASH 030.jpg', 30, 'Hawkeye, Traper a Radar vydírají jednoho poručíka, aby povolil sňatek desátníka Walkera s místní korejskou dívkou.');
INSERT INTO movie VALUES (1157, 'MASH/MASH 031.mp4', 'Kim', 11, 1, 'cz', '  ', 'en', 'Kim', 'http://www.csfd.cz/film/68850-mash/', 'MASH 031.jpg', 31, 'Traper se sblíží s domnělým korejským sirotkem, pětiletým Kimem a chce ho adoptovat. Zachrání ho z minového pole, ale nakonec se ukáže, že Kimovi rodiče žijí a odvedou si ho.');
INSERT INTO movie VALUES (1158, 'MASH/MASH 032.mp4', 'Soud Henryho Blakea', 11, 1, 'cz', '  ', 'en', 'Trial of Henry Blake', 'http://www.csfd.cz/film/68850-mash/', 'MASH 032.jpg', 32, 'Margaret a Frank tvrdí, že Henry je neschopný velitel a vsadí Hawkeye a Trapera do vazby. Ti však utečou a s pomocí sestry Crattyové z místního sirotčince, které Henry pomohl, jej ve funkci udrží.');
INSERT INTO movie VALUES (1159, 'MASH/MASH 033.mp4', 'Drahý otče... Tři', 11, 1, 'cz', '  ', 'en', 'Dear Dad... Three', 'http://www.csfd.cz/film/68850-mash/', 'MASH 033.jpg', 33, 'Opět několik příhod z všedních dnů M*A*S*He. Hawkeye a Traper obarví kůži fanatickému seržantovi - rasistovi, důstojníci mají každoměsíční poradu a Henry dostane z domova film z narozenin dcery.');
INSERT INTO movie VALUES (1160, 'MASH/MASH 034.mp4', 'Ostřelovač', 11, 1, 'cz', '  ', 'en', 'The Sniper', 'http://www.csfd.cz/film/68850-mash/', 'MASH 034.jpg', 34, 'Severokorejský ostřelovač si splete tábor se štábem generála McArthura a ochromí život v jednotce. Terčem se stane i Radar a plukovník Blake ve sprše. Nakonec ostřelovače zneškodní vrtulník a Hawkeye se vydává jej ošetřit.');
INSERT INTO movie VALUES (1161, 'MASH/MASH 035.mp4', 'Drž se, Hawkeye', 11, 1, 'cz', '  ', 'en', 'Carry On Hawkeye', 'http://www.csfd.cz/film/68850-mash/', 'MASH 035.jpg', 35, 'Chřipková epidemie postihla celý tábor s výjimkou Hawkeye a Margaret. Nakonec ale oba skončí se stejným onemocněním, byť musel Pierce podstoupit očkování, při němž mu injekci do zadku dávala k Frankově nelibosti právě Margaret.');
INSERT INTO movie VALUES (1162, 'MASH/MASH 036.mp4', 'Inkubátor', 11, 1, 'cz', '  ', 'en', 'The Incubator', 'http://www.csfd.cz/film/68850-mash/', 'MASH 036.jpg', 36, 'Hawkeye a Traper bojují s velkým úsilím s armádní mašinérií, aby pro 4077 získali potřebný inkubátor. Dostanou se kvůli tomu i do konfliktu s generálem na tiskové konferenci. Nakonec inkubátor sežene Radar, který jej vymění za nový gril plukovníka Blakea.');
INSERT INTO movie VALUES (1163, 'MASH/MASH 037.mp4', 'Dostaňte mě odtud', 11, 1, 'cz', '  ', 'en', 'Deal Me Out', 'http://www.csfd.cz/film/68850-mash/', 'MASH 037.jpg', 37, 'Kazdotýdenní partie pokru v Bažině, na kterou dojíždí do tábora psychiatr Freedman, je přerušena: Radar porazí džípem jednoho místního obyvatele, známého tím, že skáče před vojenská vozidla, aby získal odškodnění. Hawkeye a Traper operují proti předpisům důstojníka tajné služby a Freedman uklidňuje zraněného vojáka, který se pokusil zabít Franka za to, že jej chtěl poslat zpátky na frontu.');
INSERT INTO movie VALUES (1164, 'MASH/MASH 038.mp4', 'Šťabajzna a prázdná náruč', 11, 1, 'cz', '  ', 'en', 'Hot Lips and Empty Arms', 'http://www.csfd.cz/film/68850-mash/', 'MASH 038.jpg', 38, 'Major Houlihanová hodnotí svůj život a chce jej změnit. Rozhodne se rozejít s Frankem a žádá o přeložení. Na rozlučkovém večírku se opije a když zjistí, jak jsou k ní loajální její podřízené a kolik má vlastně v táboře přátel, své rozhodnutí změní.');
INSERT INTO movie VALUES (1165, 'MASH/MASH 039.mp4', 'Pouze pro důstojníky', 11, 1, 'cz', '  ', 'en', 'Officers Only', 'http://www.csfd.cz/film/68850-mash/', 'MASH 039.jpg', 39, 'Hawkeye a Traper zachrání život synovi generála Mitchella a na oplátku dostanou tři dny volna v Tokiu a důstojnický klub. Oba chytře přesvědčí generála, aby klub mohli navštěvovat všichni, bez rozdílu v hodnostech. Klinger předstírá, že je těhotný.');
INSERT INTO movie VALUES (1166, 'MASH/MASH 040.mp4', 'Zamilovaný Henry', 11, 1, 'cz', '  ', 'en', 'Henry in Love', 'http://www.csfd.cz/film/68850-mash/', 'MASH 040.jpg', 40, 'Plukovník Henry se při dovolené v Tokiu zamiluje do dvacetileté dívky jménem Nancy Sue Parkerová. Hawkeye, Traper a Radar se snaží zaslepenému Henrymu zabránit ve zničení jeho manželství. Nakonec mu napraví hlavu telefonát s manželkou.');
INSERT INTO movie VALUES (1167, 'MASH/MASH 041.mp4', 'Vše pro boty', 11, 1, 'cz', '  ', 'en', 'For Want of a Boot', 'http://www.csfd.cz/film/68850-mash/', 'MASH 041.jpg', 41, 'Hawkeye chce nové boty a je ochoten kvůli tomu udělat cokoliv. A tak nastává situace, podobná pohádce o kohoutkovi a slepičce. Aby mu zásobovač Zale dal nový pár, musí mu zařídit ošetření u táborového zubaře, který chce od Henryho dovolenou v Tokiu, Henry mu dá dovolenou jen když Margaret nepošle svou zprávu na velitelství, ta na oplátku požaduje uspořádání narozeninové oslavy s dortem pro Franka, Radar je ochoten s oslavou pomoci, jen když mu Hawkeye sjedná rande se setrou Murphyovou, která za to požaduje fén od Klingera, který chce podepsat paragraf 8. Frank ale odmítne podepsat a tak se celý plán zhroutí.');
INSERT INTO movie VALUES (1168, 'MASH/MASH 042.mp4', 'Operace Nos', 11, 1, 'cz', '  ', 'en', 'Operation Noselift', 'http://www.csfd.cz/film/68850-mash/', 'MASH 042.jpg', 42, 'Hawkeye a Traper se rozhodnout pomoci vojákovi, který trpí pro svůj obrovský nos. Pozvou do tábora Hawkeyeho přítele - plastického chirurga. Ten je ochoten operovat pouze pod podmínkou, že jej Hawkeye a Traper seznámí s nějakou sestrou. Předhodí mu tedy sestru přezdívanou Barakuda. Hlavní problém ale je, aby se to nedozvěděli Frank a Margaret.');
INSERT INTO movie VALUES (1169, 'MASH/MASH 043.mp4', 'Vybraní lidé', 11, 1, 'cz', '  ', 'en', 'The Chosen People', 'http://www.csfd.cz/film/68850-mash/', 'MASH 043.jpg', 43, 'Korejská rodina se rozhodne usadit přímo uprostřed tábora. Korejská dívka navíc označí Radara za otce svého dítěte. Rodina je nakonec umístěna do nového domova a krevní testy Radara očistí.');
INSERT INTO movie VALUES (1170, 'MASH/MASH 044.mp4', 'Jací jste byli', 11, 1, 'cz', '  ', 'en', 'As You Were', 'http://www.csfd.cz/film/68850-mash/', 'MASH 044.jpg', 44, 'Jak 4077 nemá několik dní žádné raněné, začnou se jeho stálí obyvatelé nudit - a tak se odvíjí řada bláznivých příběhů, v nichž například hraje roli Frank a kostým gorily. Jakmile začnou vrtulníky přivážet raněné, začne vlastní strana ostřelovat tábor a zasáhne generátor. Radar se vydává zastavit palbu.');
INSERT INTO movie VALUES (1171, 'MASH/MASH 045.mp4', 'Krize', 11, 1, 'cz', '  ', 'en', 'Crisis', 'http://www.csfd.cz/film/68850-mash/', 'MASH 045.jpg', 45, 'Zásobovací cesty k táboru jsou přerušeny a tak se musí se vším šetřit. Navíc je zima a tak se pálí vše "nepotřebné", aby se personál i ranění zahřáli. Nakonec jsou však dodávky i s dalším přívalem raněných obnoveny.');
INSERT INTO movie VALUES (1172, 'MASH/MASH 046.mp4', 'George', 11, 1, 'cz', '  ', 'en', 'George', 'http://www.csfd.cz/film/68850-mash/', 'MASH 046.jpg', 46, 'Frank zjistí, že jeden z pacientů je homosexual a snaží se dosáhnout toho, aby byl zmíněný voják s hanbou vyhozen z armády.');
INSERT INTO movie VALUES (1210, 'MASH/MASH 084.mp4', 'Autobus', 11, 1, 'cz', '  ', 'en', 'The Bus', 'http://www.csfd.cz/film/68850-mash/', 'MASH 084.jpg', 84, 'Radar přiváží autobusem Hawkeyho a ostatní z lékařského kongresu, ale ztratí se a navíc se poláme autobus. Nakonec jsou zachráněni zraněným Korejcem.');
INSERT INTO movie VALUES (1173, 'MASH/MASH 047.mp4', 'Pošta volá', 11, 1, 'cz', '  ', 'en', 'Mail Call', 'http://www.csfd.cz/film/68850-mash/', 'MASH 047.jpg', 47, 'Dopis z domova vyvolá v Traperovi depresi a touhu - rozhodne se dezertovat, přestože se mu v tom Hawkeye snaží zabránit. Mezitím se Hawkeymu podaří úspěšně napálit Franka - přemluví jej, aby koupil akcie fiktivní společnosti - Pioneer Aviation.');
INSERT INTO movie VALUES (1174, 'MASH/MASH 048.mp4', 'Souboj agentů', 11, 1, 'cz', '  ', 'en', 'A Smattering of Intelligence', 'http://www.csfd.cz/film/68850-mash/', 'MASH 048.jpg', 48, 'Pomatený plukovník Flagg a jiný tajný agent z jiné zpravodajské služby se vydají do tábora, aby tajně sledovali jeden druhého. Hawkeye a Traper způsobí, že si oba agenti myslí, ze Frank Burns je zrádce - jeden ho považuje za fašistu a druhý za komunistu.');
INSERT INTO movie VALUES (1175, 'MASH/MASH 049.mp4', 'Duhový most', 11, 1, 'cz', '  ', 'en', 'Rainbow Bridge', 'http://www.csfd.cz/film/68850-mash/', 'MASH 049.jpg', 49, 'Hawkeye a Traper plánují dovolenou v Tokiu, když přijde nabídka na výměnu amerických raněných za čínské zajatce a jednotka 4077 má výměnu provést. Henry se rozhodne vyslat Hawkeyeho, Trapera, Burnse, Radara a Klingera na misi na nepřátelské území. Frank málem všechno pokazí, když si přes striktní příkaz vezme stříkací pistolku. Naštěstí čínský doktor projeví dostatek smyslu pro humor - asi proto, že studoval na univerzitě v Illinois.');
INSERT INTO movie VALUES (1176, 'MASH/MASH 050.mp4', 'Život s Otcem', 11, 1, 'cz', '  ', 'en', 'Life with Father', 'http://www.csfd.cz/film/68850-mash/', 'MASH 050.jpg', 50, 'Velitel Henry dostane z domova dopis, ze kterého si vydedukuje, že jeho žena má možná jiného muže. Otec Mulcahy provede židovský obřad křtu pro korejské dítě amerického vojáka.');
INSERT INTO movie VALUES (1177, 'MASH/MASH 051.mp4', 'Jaro', 11, 1, 'cz', '  ', 'en', 'Springtime', 'http://www.csfd.cz/film/68850-mash/', 'MASH 051.jpg', 51, 'S příchodem jara dostává Klinger z domova dopis od své snoubenky, že by si jej v Toledu chtěla vzít. Otec Mulcahy (s Radarovou pomoci) zařídí svatbu pomocí krátkovlného rádia. Radar se zamiluje do sestry, mezitím vděčný pacient nechce Hawkeyho nechat samotného a neustále se mu snaží pomáhat. Prostě přišlo jaro.');
INSERT INTO movie VALUES (1178, 'MASH/MASH 052.mp4', 'Železňák Kelly', 11, 1, 'cz', '  ', 'en', 'Iron Guts Kelly', 'http://www.csfd.cz/film/68850-mash/', 'MASH 052.jpg', 52, 'Generál ''Železňák'' Kelly přijede na inspekci a nakonec skončí v Margaretině stanu. Bohužel, v náručí majorky Houlihanové nečekaně zemře a jeho pobočník se snaží zařídit vše tak, aby to vypadalo, že generál zemřel ve velkolepé bitvě. Hawkeye a Traper mu pomáhají propašovat generálovo tělo z tábora.');
INSERT INTO movie VALUES (1179, 'MASH/MASH 053.mp4', 'Výplata', 11, 1, 'cz', '  ', 'en', 'Payday', 'http://www.csfd.cz/film/68850-mash/', 'MASH 053.jpg', 53, 'Při výplatě dostane Hawkeye omylem 3000 dolarů jako odškodnění za to, že musí být ve válce. Daruje je otci Mulcahymu pro sirotky, ale armáda chce peníze zpátky. Traper se snaží Hawkeyeho zachránit před vězením tím, že vyhraje peníze v pokeru (předtím si ale do zástavy půjčí jeho hodinky) a Frank koupí své ženě perlový náhrdelník a Margaretě daruje levnou napodobeninu.');
INSERT INTO movie VALUES (1180, 'MASH/MASH 054.mp4', 'Operační den', 11, 1, 'cz', '  ', 'en', 'O.R', 'http://www.csfd.cz/film/68850-mash/', 'MASH 054.jpg', 54, 'V operační den jednotka musí ošetřit více zraněných, než je schopna zvládnout. Doktorům musí vypomáhat i psychiatr Dr. Freedmann. Ten se nakonec stará i o Hawkeyeho, který sice zachránil masáží srdce pacienta, ale ten stejně za několik hodin na jiné zranění zemřel.');
INSERT INTO movie VALUES (1181, 'MASH/MASH 055.mp4', 'Dozorčí dne', 11, 1, 'cz', '  ', 'en', 'Officer of the Day', 'http://www.csfd.cz/film/68850-mash/', 'MASH 055.jpg', 55, 'V době Henryho cesty do Soulu přebírají velení Frank a Margaret. Hawkeyeho určí dozorčím dne. K jeho smůle se objeví plukovník Flagg, s nímž se Hawkeye dostane do konfliktu poté, co mu odmítne vydat zraněného korejského vojáka, o kterého se zajímá tajná služba.');
INSERT INTO movie VALUES (1182, 'MASH/MASH 056.mp4', 'Generál za úsvitu', 11, 1, 'cz', '  ', 'en', 'The General Flipped at Dawn', 'http://www.csfd.cz/film/68850-mash/', 'MASH 056.jpg', 56, 'I Klinger dojde k názoru, že generál Bartford Steele je blázen, když si ho splete se svou manželkou. Šílenství u něj vypuklo následkem posedlosti vojenskou disciplínou.');
INSERT INTO movie VALUES (1183, 'MASH/MASH 057.mp4', 'Nad sestru není', 11, 1, 'cz', '  ', 'en', 'There is Nothing Like a Nurse', 'http://www.csfd.cz/film/68850-mash/', 'MASH 057.jpg', 57, 'Všechny sestry ze 4077 jsou evakuovány před hrozbou nepřátelského náletu. Hawkeye s Traperem se snaží přežít nudu pomocí nejrůznějších vtípků - většinou na úkor Franka.');
INSERT INTO movie VALUES (1184, 'MASH/MASH 058.mp4', 'Vojín Charles Jehně', 11, 1, 'cz', '  ', 'en', 'Private Charles Lamb', 'http://www.csfd.cz/film/68850-mash/', 'MASH 058.jpg', 58, 'Řecký plukovník poděkuje jednotce jídlem a pitím na velikonoční oslavu. Ale slavnost je ohrožena poté, co se Radar rozhodne zachránit hlavní chod hostiny - jehně. Pomocí triku pošle jehně jako fiktivního vojína zpět domů do Ottumwy. Naštěstí Hawkeye a Traper seženou náhradu. Zajímavý příběh prožije i Frank, když si jej vojín, který se sám postřelí, splete s kaplanem a vyzpovídá se mu.');
INSERT INTO movie VALUES (1185, 'MASH/MASH 059.mp4', 'Všední den', 11, 1, 'cz', '  ', 'en', 'A Full Rich Day', 'http://www.csfd.cz/film/68850-mash/', 'MASH 059.jpg', 59, 'Hawkeye nahrává pro svého otce dopis, ve kterém popisuje snahu tureckého vojáka, který se přes své zranění chce vrátit k jednotce. Dále popisuje pátrání po domněle zemřelém lucemburském vojákovi poručíkovi Henri-Batiste LeClercovi, který se nakonec objeví živý.');
INSERT INTO movie VALUES (1186, 'MASH/MASH 060.mp4', 'Kontrola', 11, 1, 'cz', '  ', 'en', 'Check-Up', 'http://www.csfd.cz/film/68850-mash/', 'MASH 060.jpg', 60, 'Při pravidelné lékařské kontrole se zjistí, že Traper má vřed, což by mu mělo zajistit lístek domů. Bohužel během oslavy jeho odchodu se dozvídá, že podle nového armádního rozkazu má nárok pouze na 3 týdny léčení.');
INSERT INTO movie VALUES (1187, 'MASH/MASH 061.mp4', 'Big Mac', 11, 1, 'cz', '  ', 'en', 'Big Mac', 'http://www.csfd.cz/film/68850-mash/', 'MASH 061.jpg', 61, 'Celý tábor se připravuje na návštěvu generála MacArthura. Klinger se převleče za Sochu svobody a když džíp s generálem projíždí kolem, MacArthur je tak překvapen, že dokonce zasalutuje.');
INSERT INTO movie VALUES (1188, 'MASH/MASH 062.mp4', 'Alkoholici', 11, 1, 'cz', '  ', 'en', 'Alcoholics Unanimous', 'http://www.csfd.cz/film/68850-mash/', 'MASH 062.jpg', 62, 'Henry je služebně v Tokiu a velitelem 4077 je major Burns. Jedním z opatření, která vydá k posílení kázně a pořádku, je totální prohibice. Toto nařízení vyvolá silný odpor, jehož hlavními iniciátory jsou Hawkeye a Traper.');
INSERT INTO movie VALUES (1189, 'MASH/MASH 063.mp4', 'Domácí vězení', 11, 1, 'cz', '  ', 'en', 'House Arrest', 'http://www.csfd.cz/film/68850-mash/', 'MASH 063.jpg', 63, 'Hawkeye urazí majora Burnse a Margaret je svědek. Frank podá na Hawkeye žalobu, a ten je prozatím odsouzen k domácímu vězení. Do tábora přijíždí plukovnice, která má incident vyšetřit. Poté, co ji Frank navštíví ve stanu, obviní ho ze znásilnění. Margaret prohlásí, že Hawkeye je nevinnen, a tak nakonec v domácím vězení skončí Frank.');
INSERT INTO movie VALUES (1211, 'MASH/MASH 085.mp4', 'Kam kráčíte, kapitáne Chandlere?', 11, 1, 'cz', '  ', 'en', 'Quo Vadis, Captain Chandler?', 'http://www.csfd.cz/film/68850-mash/', 'MASH 085.jpg', 85, 'Tajný agent plukovník Flagg a psychiatr Sidney Freedman spolu bojují o zraněného vojáka, který je přesvědčen, že je Ježíš Kristus.');
INSERT INTO movie VALUES (1190, 'MASH/MASH 064.mp4', 'Adamova žebra', 11, 1, 'cz', '  ', 'en', 'Adam´s Ribs', 'http://www.csfd.cz/film/68850-mash/', 'MASH 064.jpg', 64, 'Hawkeye na pokraji šílenství zahájí v jídelně protestní demonstraci, protože během deseti dnů byl nucen sníst "řeku jater a oceán ryb". Aby si vylepšili jídelníček, zahájí Hawkeye, Traper a Radar akci, jíž má být dodávka žebírek z vyhlášené Chicagské restaurace. To se jim nakonec podaří - bohužel žebra dojdou právě ve chvíli, kdy do tábora začínají proudit noví zranění.');
INSERT INTO movie VALUES (1191, 'MASH/MASH 065.mp4', 'Zlí psi a servismani', 11, 1, 'cz', '  ', 'en', 'Mad Dogs and Servicemen', 'http://www.csfd.cz/film/68850-mash/', 'MASH 065.jpg', 65, 'Radar je pokousán místním psem a celý tábor se ho snaží najít, aby Radar nemusel podstoupit sérii bolestivých injekcí. Hawkeye přes Frankův odpor pečuje o vojáka, který utrpěl v boji hysterickou paralýzu a nemůže se hýbat. Nakonec s pomocí nové metody dr. Friedmana slaví úspěch.');
INSERT INTO movie VALUES (1192, 'MASH/MASH 066.mp4', 'Konzultant', 11, 1, 'cz', '  ', 'en', 'The Consultant', 'http://www.csfd.cz/film/68850-mash/', 'MASH 066.jpg', 66, 'Dr. Borelli navštíví 4077, aby demonstroval novou techniku arteriální transplantace. Ale blízkost fronty u něho znovuoživí dávný problém s alkoholem a opije se v nejnevhodnější čas - před operací, a tak musí operovat Hawkeye.');
INSERT INTO movie VALUES (1193, 'MASH/MASH 067.mp4', 'Bílé zlato', 11, 1, 'cz', '  ', 'en', 'White Gold', 'http://www.csfd.cz/film/68850-mash/', 'MASH 067.jpg', 67, 'Plukovník Flagg přijíždí do tábora, aby se pokusil ukrást penicilín, za který si kupuje informace. Musí ale podstoupit operaci slepého střeva a tak jediný penicilin, který dostane, je z injekční stříkačky.');
INSERT INTO movie VALUES (1194, 'MASH/MASH 068.mp4', 'Bombardovaní', 11, 1, 'cz', '  ', 'en', 'Bombed', 'http://www.csfd.cz/film/68850-mash/', 'MASH 068.jpg', 68, 'Tábor je několik hodin pod palbou a je zaplaven zraněnými. Major Burns žárlí na Trapera, který strávil několik hodin se Štabajznou v zamčeném skladu.');
INSERT INTO movie VALUES (1195, 'MASH/MASH 069.mp4', 'Láska a svatba', 11, 1, 'cz', '  ', 'en', 'Love and Marriage', 'http://www.csfd.cz/film/68850-mash/', 'MASH 069.jpg', 69, 'Hawkeye a Traper zabrání americkému vojákovi ve svatbě s místní prostitutkou, která má tuberkulózu. Také s Radarovou pomocí zajistí návštěvu těhotné manželky korejského vojáka.');
INSERT INTO movie VALUES (1196, 'MASH/MASH 070.mp4', 'První pomoc', 11, 1, 'cz', '  ', 'en', 'Aid Station', 'http://www.csfd.cz/film/68850-mash/', 'MASH 070.jpg', 70, 'Hawkeye, Houlihanová a Klinger jdou na frontu vypomoci stanici první pomoci. Práce pod silnou palbou v těžkých podmínkách vede k tomu, že všichni tři k sobě vzájemně nacházejí nový respekt.');
INSERT INTO movie VALUES (1197, 'MASH/MASH 071.mp4', 'Nástěnka', 11, 1, 'cz', '  ', 'en', 'Bulletin Boards', 'http://www.csfd.cz/film/68850-mash/', 'MASH 071.jpg', 71, 'Nudu v táboře dokáže odstranit jen Henryho pravidelná přednáška o sexu, při které Hawkeye s Traperem velmi vyrušují. Dalším zpestřením života v táboře je film se Shirley Templeovou.');
INSERT INTO movie VALUES (1198, 'MASH/MASH 072.mp4', 'Abyssinia, Henry', 11, 1, 'cz', '  ', 'en', 'Abyssinia, Henry', 'http://www.csfd.cz/film/68850-mash/', 'MASH 072.jpg', 72, 'Henry se konečně dočká převelení do Spojených států. Mezitím, co si Henry zařizuje poslední věci na převzetí velitelské funkce, se už chystá Frank Burns. Při rozloučení musí Henry ještě zapnout Klingerovi šaty, ušité speciálně pro tuto příležitost, a také stihne dlouze políbit Šťabajznu. Nakonec dá ještě poslední rozkaz Radarovi: "Radši tady buď hodný, nebo se vrátím a nakopu tě do zadku". V šokující poslední scéně přichází Radar se zprávou, že Henryho letadlo bylo sestřeleno nad Japonským mořem a nikdo nepřežil...');
INSERT INTO movie VALUES (1199, 'MASH/MASH 073.mp4', 'Vítej v Koreji 1', 11, 1, 'cz', '  ', 'en', 'Welcome to Korea 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 073.jpg', 73, 'Frankův sen se splnil - převzal velení, ale pořád má starosti s nedisciplinovanými podřízenými. Například Hawkeye je nepolepšitelný - opouští tábor, aby se rozloučil s Traperem, který je konečně také převelen do Států. Trapera už sice nestihne, ale potkává nového sympatického chirurga - B.J. Hunnicuta.');
INSERT INTO movie VALUES (1200, 'MASH/MASH 074.mp4', 'Vítej v Koreji 2', 11, 1, 'cz', '  ', 'en', 'Welcome to Korea 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 074.jpg', 74, 'Frankův sen se splnil - převzal velení, ale pořád má starosti s nedisciplinovanými podřízenými. Například Hawkeye je nepolepšitelný - opouští tábor, aby se rozloučil s Traperem, který je konečně také převelen do Států. Trapera už sice nestihne, ale potkává nového sympatického chirurga - B.J. Hunnicuta.');
INSERT INTO movie VALUES (1201, 'MASH/MASH 075.mp4', 'Změna velení', 11, 1, 'cz', '  ', 'en', 'Change of Command', 'http://www.csfd.cz/film/68850-mash/', 'MASH 075.jpg', 75, 'Frank si rychle zvyká na místo velícího důstojníka, jenže přijíždí nový plukovník, veterán Sherman Potter. To, že je sesazen, vede u Franka k těžké depresi.');
INSERT INTO movie VALUES (1202, 'MASH/MASH 076.mp4', 'Stalo se jedné noci', 11, 1, 'cz', '  ', 'en', 'It Happened One Night', 'http://www.csfd.cz/film/68850-mash/', 'MASH 076.jpg', 76, 'Během mrazivé noci se dostane dělostřelecká palba velmi blízko k táboru, což způsobí zhoršení zdravotního stavu několika pacientů. Frank se dozví, že Margaret si schovává všechny jeho dopisy a rozhodne se, je zničit, přičemž obrátí Šťabajznin stan naruby.');
INSERT INTO movie VALUES (1203, 'MASH/MASH 077.mp4', 'O paroháčích a lidech', 11, 1, 'cz', '  ', 'en', 'Of Moose and Men', 'http://www.csfd.cz/film/68850-mash/', 'MASH 077.jpg', 77, 'Hawkeye se dostane do sporu s přísným armádním plukovníkem Spikesem, B.J pomáhá vojákovi, který dostal nepříjemný dopis od manželky (že se s ním rozchází) a Frank konečně odhaluje korejské "sabotéry".');
INSERT INTO movie VALUES (1204, 'MASH/MASH 078.mp4', 'Drahá Mildred', 11, 1, 'cz', '  ', 'en', 'Dear Mildred', 'http://www.csfd.cz/film/68850-mash/', 'MASH 078.jpg', 78, 'Plukovník Potter píše dopis své manželce k výročí sňatku. Frank a Šťabajzna se rozhodnou věnovat mu při té příležitosti dřevěnou bustu, ale největší radost mu udělá Radar, který zachrání koně a daruje mu ho.');
INSERT INTO movie VALUES (1205, 'MASH/MASH 079.mp4', 'Mrtvý kapitán Pierce', 11, 1, 'cz', '  ', 'en', 'The Late Captain Pierce', 'http://www.csfd.cz/film/68850-mash/', 'MASH 079.jpg', 79, 'Hawkeyho otec je chybně informován o synově smrti a Hawkeye dělá vše proto, aby věci uvedl na provou míru.');
INSERT INTO movie VALUES (1206, 'MASH/MASH 080.mp4', 'Smějící se Jack', 11, 1, 'cz', '  ', 'en', 'Smilin´ Jack', 'http://www.csfd.cz/film/68850-mash/', 'MASH 080.jpg', 80, 'Hawkeye a B.J zjistí, že pilot vrtulníku má cukrovku a přes jeho odpor musí být odvelen, zatímco dvakrát zraněný voják musí po vyléčení zpátky na frontu.');
INSERT INTO movie VALUES (1207, 'MASH/MASH 081.mp4', 'Drahá Peggy', 11, 1, 'cz', '  ', 'en', 'Dear Peggy', 'http://www.csfd.cz/film/68850-mash/', 'MASH 081.jpg', 81, 'B.J. píše domů své ženě. V dopise popisuje Klingerovy pokusy o útěk, návštěvu bláznivého kaplana a jeden z Frankových hysterických výstupů.');
INSERT INTO movie VALUES (1208, 'MASH/MASH 082.mp4', 'Hej doktore', 11, 1, 'cz', '  ', 'en', 'Hey, Doc', 'http://www.csfd.cz/film/68850-mash/', 'MASH 082.jpg', 82, 'Půjčky za oplátku ve 4077: dvě láhve skotské whisky za tajnou operaci a tank na zastrašení ostřelovačů za nepovolenou injekci penicilínu.');
INSERT INTO movie VALUES (1209, 'MASH/MASH 083.mp4', 'Děti', 11, 1, 'cz', '  ', 'en', 'The Kids', 'http://www.csfd.cz/film/68850-mash/', 'MASH 083.jpg', 83, 'Jednotka 4077 hostí několik dnů sirotky, kteří se musí ukrýt před bombardováním. Ve stejné době zachrání život postřelené těhotné žene i jejímu dítěti, kterému navíc udělí řád purpurového srdce.');
INSERT INTO movie VALUES (1212, 'MASH/MASH 086.mp4', 'Vojín měsíce', 11, 1, 'cz', '  ', 'en', 'Soldier of the Month', 'http://www.csfd.cz/film/68850-mash/', 'MASH 086.jpg', 86, 'V táboře se rozšíří neznámá nemoc, kterou dostane i Frank a v horečce píše závěť, ve které odkáže všechny peníze své ženě a všechno oblečení Šťabajzně. Radar je zvolen vojínem měsíce a za odměnu dostane dovolenou v Tokiu.');
INSERT INTO movie VALUES (1213, 'MASH/MASH 087.mp4', 'Drahá mami', 11, 1, 'cz', '  ', 'en', 'Dear Ma', 'http://www.csfd.cz/film/68850-mash/', 'MASH 087.jpg', 87, 'Radar píše dopis domů své matce o životě v jednotce. Popisuje Hawkeyho celotáborovou inspekci nohou a také zranění plukovníka Pottera, který je postřelen do zadku.');
INSERT INTO movie VALUES (1214, 'MASH/MASH 088.mp4', 'Probuzení', 11, 1, 'cz', '  ', 'en', 'Deluge', 'http://www.csfd.cz/film/68850-mash/', 'MASH 088.jpg', 88, 'Velký nápor zraněných v táboře je doprovázen střelbou a bouří. Do války se zapojuje Čína, což znamená konec nadějí na příměří.');
INSERT INTO movie VALUES (1215, 'MASH/MASH 089.mp4', 'Zbraň', 11, 1, 'cz', '  ', 'en', 'The Gun', 'http://www.csfd.cz/film/68850-mash/', 'MASH 089.jpg', 89, 'V táboře se ztratí oblíbená osobní zbraň zraněného plukovníka a Radarovi za to hrozí vězení. Hawkeye a B.J. se snaží přimět skutečného pachatele - Franka, aby zbraň vrátil.');
INSERT INTO movie VALUES (1216, 'MASH/MASH 090.mp4', 'Znovu volání pošty', 11, 1, 'cz', '  ', 'en', 'Mail Call, Again', 'http://www.csfd.cz/film/68850-mash/', 'MASH 090.jpg', 90, 'Frank dostává dopis, v němž mu manželka sděluje, že se chce rozvést. Také plukovník Potter dostává dopis a dozvídá se, že se stal dědečkem.');
INSERT INTO movie VALUES (1217, 'MASH/MASH 091.mp4', 'Cena tomatového džusu', 11, 1, 'cz', '  ', 'en', 'The Price of Tomato Juice', 'http://www.csfd.cz/film/68850-mash/', 'MASH 091.jpg', 91, 'Radar s pomocí Hawkeyho a B.J. sežene po velké námaze pro plukovníka Pottera jeho oblíbený tomatový džus, ale nakonec vyjde najevo, že plukovník kvůli alergii nemůže džus pít.');
INSERT INTO movie VALUES (1218, 'MASH/MASH 092.mp4', 'Hawkeye', 11, 1, 'cz', '  ', 'en', 'Hawkeye', 'http://www.csfd.cz/film/68850-mash/', 'MASH 092.jpg', 92, 'Hawkeye havaruje s džípem a je zraněn na hlavě. Protože ví, že nesmí upadnout do bezvědomí, celý den mluví ke korejské rodině i když mu nikdo z ní nerozumí.');
INSERT INTO movie VALUES (1219, 'MASH/MASH 093.mp4', '38 rovnoběžka', 11, 1, 'cz', '  ', 'en', 'Some 38th Parallels', 'http://www.csfd.cz/film/68850-mash/', 'MASH 093.jpg', 93, 'Ziskuchtivý Frank se rozhodne v dražbě prodávat Korejcům odpadky z táborové skládky, ale nakonec je to Hawkeye, který najde jejich využití. Nechá je shodit z vrtulníku na fanatického plukovníka Coner.');
INSERT INTO movie VALUES (1220, 'MASH/MASH 094.mp4', 'Der Tag', 11, 1, 'cz', '  ', 'en', 'Der Tag', 'http://www.csfd.cz/film/68850-mash/', 'MASH 094.jpg', 94, 'Plukovník Potter rozhodne, že by se Hawkeye a ostatní měli chovat více přátelsky k Frankovi, který je v depresi. Když se tak stane, Frank stihne obehrát všechny v pokeru, opít se v baru a ještě prospí svůj výlet na frontu.');
INSERT INTO movie VALUES (1221, 'MASH/MASH 095.mp4', 'Novocainová vzpoura', 11, 1, 'cz', '  ', 'en', 'The Novocaine Mutiny', 'http://www.csfd.cz/film/68850-mash/', 'MASH 095.jpg', 95, 'Frank obžaluje Hawkeyho ze vzpoury poté, co plukovník Potter odjel na dovolenou a Frank jako velící důstojník prováděl se 4077 šílené věci, až to Hawkeye nesnesl.');
INSERT INTO movie VALUES (1222, 'MASH/MASH 096.mp4', 'Až tě zase uvidím', 11, 1, 'cz', '  ', 'en', 'The More I See You', 'http://www.csfd.cz/film/68850-mash/', 'MASH 096.jpg', 96, 'Hawkeye se setkává se ženou, o které si myslel, že odešla z jeho života navždy, ale kterou ve skutečnosti nikdy nepřestal milovat.');
INSERT INTO movie VALUES (1223, 'MASH/MASH 097.mp4', 'Interview', 11, 1, 'cz', '  ', 'en', 'The interview', 'http://www.csfd.cz/film/68850-mash/', 'MASH 097.jpg', 97, 'Televizní zpravodaj z USA pořádá interview se členy M*A*S*H (černobíle).');
INSERT INTO movie VALUES (1224, 'MASH/MASH 098.mp4', 'Stěhování 1', 11, 1, 'cz', '  ', 'en', 'Bug Out 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 098.jpg', 98, 'Po útoku Číňanů je prolomena fronta a tábor se musí přestěhovat dál od místa bojů. V táboře je ale jeden pacient, který není schopen převozu, a tak s ním Hawkeye, Margaret a Radar zůstanou na místě. Nakonec jsou Číňané zahnáni zpět a MASH se vrací na staré místo.');
INSERT INTO movie VALUES (1225, 'MASH/MASH 099.mp4', 'Stěhování 2', 11, 1, 'cz', '  ', 'en', 'Bug Out 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 099.jpg', 99, 'Po útoku Číňanů je prolomena fronta a tábor se musí přestěhovat dál od místa bojů. V táboře je ale jeden pacient, který není schopen převozu, a tak s ním Hawkeye, Margareta a Radar zůstanou na místě. Nakonec jsou Číňané zahnáni zpět a MASH se vrací na staré místo.');
INSERT INTO movie VALUES (1226, 'MASH/MASH 100.mp4', 'Margaretino zasnoubení', 11, 1, 'cz', '  ', 'en', 'Margaret´s Engagement', 'http://www.csfd.cz/film/68850-mash/', 'MASH 100.jpg', 100, 'Major Houlihanová volá z Tokia, že má vynikající zprávu. Proto všichni napjatě očekávají její návrat. Po příjezdu Margaret oznámí svoje zasnoubení s podplukovníkem Donaldem Penobscottem. Frank tuto zprávu nese velmi těžce, odejde z tábora a přivede korejskou rodinu jako válečné zajatce - obviní je ze špionáže.');
INSERT INTO movie VALUES (1227, 'MASH/MASH 101.mp4', 'Hawkeyho noční můra', 11, 1, 'cz', '  ', 'en', 'Hawk´s Nightmare', 'http://www.csfd.cz/film/68850-mash/', 'MASH 101.jpg', 101, 'Hawkeyho začnou trápit zlé sny a náměsíčnictví. Jeho sny se většinou odehrávají v mládí a končí nějakou nehodou. Až po příjezdu doktora Freedmana a po rozhovoru s ním se Hawkeye vrací do normálního stavu.');
INSERT INTO movie VALUES (1228, 'MASH/MASH 102.mp4', 'Poručík Radar O''Reilly', 11, 1, 'cz', '  ', 'en', 'Lt.Radar O´Reilly', 'http://www.csfd.cz/film/68850-mash/', 'MASH 102.jpg', 102, 'Tábor navštíví seržant Woodruff a poté, co nemá na zaplacení svého dluhu z pokeru, slíbí Hawkeymu a B.J., že nechá povýšit Radara na poručíka. Radar je nejprve potěšen, ale když zjistí, jaké to má výhody a nevýhody, je rád, že může být opět desátníkem.');
INSERT INTO movie VALUES (1229, 'MASH/MASH 103.mp4', 'Sejde z očí, sejde z mysli', 11, 1, 'cz', '  ', 'en', 'Out of Sight, Out of Mind', 'http://www.csfd.cz/film/68850-mash/', 'MASH 103.jpg', 103, 'Hawkeye je ošklivě zraněn v obličeji při manipulaci s kamny, které vybuchnou. Poté leží několik dnů se zabandážovaným obličejem a není jisté, jestli ještě někdy uvidí. Naštěstí po sundání obvazu Hawkeye opět vidí.');
INSERT INTO movie VALUES (1230, 'MASH/MASH 104.mp4', 'Generálův osobní lékař', 11, 1, 'cz', '  ', 'en', 'The General´s Practitioner', 'http://www.csfd.cz/film/68850-mash/', 'MASH 104.jpg', 104, 'Hawkeye dostane nabídku, aby dělal osobního lékaře pro generála, ale odmítne ji, protože na sále v táboře je užitečnější než někde v Tokiu. Mezitím se Radar na pár dní stává náhradním otcem korejského dítěte, než se jeho pravý otec vrátí.');
INSERT INTO movie VALUES (1231, 'MASH/MASH 105.mp4', 'Únos Margarety Houlihanove', 11, 1, 'cz', '  ', 'en', 'The Abduction of Margaret Houlihan', 'http://www.csfd.cz/film/68850-mash/', 'MASH 105.jpg', 105, 'Poté, co se roznese, že v oblasti se pohybuje severokorejský uprchlý vězeň, vyvolá znepokojení Margaretino zmizení. Ta ale není unesena - odchází do korejské vesnice pomáhat při porodu. Plukovník Potter povolává plukovníka Flagga z CIA. Ten už začíná plánovat velké tažení na osvobození Šťabajzny, která se ale mezitím vrátí, aniž cokoliv tuší.');
INSERT INTO movie VALUES (1543, '201208/Finley 6-8 Je tam nejaky kralicek.mp4', 'Je tam nejaky kralicek?', 3, 30, 'sk', NULL, 'en', NULL, NULL, 'Finley 6-8 Je tam nejaky kralicek.jpg', 68, NULL);
INSERT INTO movie VALUES (1602, '201212/Bolek a Lolek - pohadky - Cervena karkulka.mp4', '

Cervena karkulka', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Cervena karkulka.jpg', 62, NULL);
INSERT INTO movie VALUES (1232, 'MASH/MASH 106.mp4', 'Sestry', 11, 1, 'cz', '  ', 'en', 'The Nurses', 'http://www.csfd.cz/film/68850-mash/', 'MASH 106.jpg', 106, 'Margaret má konflikt se sestrou Bakerovou a zakáže jí pohyb po táboře. Jenže právě tehdy přijede její manžel. Hawkeye a B.J pro ně zařídí společnou noc v Margaretině stanu, o kterém všem řeknou, že je v karanténě. Když se to Margareta druhý den dozví, změkne a pokusí se o usmíření se sestrami.');
INSERT INTO movie VALUES (1233, 'MASH/MASH 107.mp4', 'Drahý Sigmunde', 11, 1, 'cz', '  ', 'en', 'Dear Sigmund', 'http://www.csfd.cz/film/68850-mash/', 'MASH 107.jpg', 107, 'Psychiatr Sidney Freedman přijede do tábora, aby se zbavil své deprese a současně sledoval, jak se s válkou vyrovnávají ostatní. Jako formu terapie zvolí psaní imaginárního dopisu Sigmundu Freudovi. Zjišťuje, že nejlepší způsob je humor.');
INSERT INTO movie VALUES (1234, 'MASH/MASH 108.mp4', 'Plukovníkův kůň', 11, 1, 'cz', '  ', 'en', 'The Colonel´s Horse', 'http://www.csfd.cz/film/68850-mash/', 'MASH 108.jpg', 108, 'Když plukovník Potter odjede na dovolenou do Tokia, v táboře se dějí divné věci - jeho kůň dostane koliku, Klinger upadne do těžké deprese a Margaret dostane zánět slepého střeva. Kůň je nakonec vyléčen, Hawkeye vyoperuje Margaretě slepé střevo a po návratu plukovníka Pottera je vše v pořádku. Kýženého převelení se mohl dočkat i Klinger, ale poté, co mu plukovník oznámí, že kvůli depresím bude odeslán domů, začne se tak radovat, že si tím vše pokazí.');
INSERT INTO movie VALUES (1235, 'MASH/MASH 109.mp4', 'Mulcahyho válka', 11, 1, 'cz', '  ', 'en', 'Mulcahy´s War', 'http://www.csfd.cz/film/68850-mash/', 'MASH 109.jpg', 109, 'Poté, co Frank zjistí, že se jeden voják sám postřelil, aby nemusel na frontu, je přivolán otec Mulcahy. Aby zažil na vlastní kůži, jaké to je, být v boji, jede načerno společně s Radarem naložit zraněného vojáka do místa bojů. Při převozu se voják začne dusit a Mulcahy mu s Hawkeyho pomocí prostřednictvím vysílačky provede tracheotomii, čímž mu zachrání život.');
INSERT INTO movie VALUES (1236, 'MASH/MASH 110.mp4', 'Hawkeye vezmi zbraň', 11, 1, 'cz', '  ', 'en', 'Hawkeye Get Your Gun', 'http://www.csfd.cz/film/68850-mash/', 'MASH 110.jpg', 110, 'Hawkeye a Potter jedou vypomáhat chirurgům do korejské nemocnice. Hawkeye se vzpírá proti tomu, aby si vzal zbraň, ale nakonec plukovníkově tlaku podlehne. Po pomoci Korejcům se vrací zpět, ale jsou ostřelováni a jejich džíp je zničen. Nakonec Hawkeye musí přece jen vystřelit, i když jen do vzduchu a v sebeobraně.');
INSERT INTO movie VALUES (1237, 'MASH/MASH 111.mp4', 'Korejský chirurg', 11, 1, 'cz', '  ', 'en', 'The Korean Surgeon', 'http://www.csfd.cz/film/68850-mash/', 'MASH 111.jpg', 111, 'Do tábora je přivezen zraněný severokorejský chirurg dr. Syn Paik, který má být po vyléčení odvezen do zajateckého tábora. Ale Hawkeye s B.J. zařídí, že dostane americké papíry a může nastoupit v 4077 jako chirurg. Margaret a Frank zjistí, že něco není v pořádku, tvrdí plukovníkovi, že je to špion a nakonec je dr. Paika nucen tábor opustit.');
INSERT INTO movie VALUES (1238, 'MASH/MASH 112.mp4', 'Vymítání', 11, 1, 'cz', '  ', 'en', 'Exorcism', 'http://www.csfd.cz/film/68850-mash/', 'MASH 112.jpg', 112, 'Poté, co plukovník Potter nařídí Radarovi, aby odstranil korejský talisman na vyhánění duchů, začnou se dít v táboře podivné věci. Začínají se kupit podivné nehody. Až poté, co je povolána místní domorodá vymítačka duchů, se vše uklidní. A Radar vrací talisman na původní místo.');
INSERT INTO movie VALUES (1239, 'MASH/MASH 113.mp4', 'Poslední běh', 11, 1, 'cz', '  ', 'en', 'End Run', 'http://www.csfd.cz/film/68850-mash/', 'MASH 113.jpg', 113, 'Billy Tyler, mladý černošský seržant, je přivezen do tábora se zraněnou nohou. V civilu je profesionálním fotbalovým hráčem a když zjistí, že mu byla noha amputována, chce zemřít. Teprve po rozhovoru s Radarem pochopí, že musí žít.');
INSERT INTO movie VALUES (1240, 'MASH/MASH 114.mp4', 'Ping Pong', 11, 1, 'cz', '  ', 'en', 'Ping Pong', 'http://www.csfd.cz/film/68850-mash/', 'MASH 114.jpg', 114, 'Podplukovník Harold Becket leží zraněný v táboře, ale chce se vrátit na frontu, aby si odsloužil zbývajících 30 dní služby v boji a mohl jít do důchodu jako plukovník. Mezitím se armádní přeborník v ping pongu Cho Lin zasnoubí s Korejkou a chce si ji vzít. Když odjede koupit snubní prstýnky, je naverbován do korejské armády a do tábora se vrací zraněný. Nakonec se uzdraví a v táboře se koná svatba.');
INSERT INTO movie VALUES (1241, 'MASH/MASH 115.mp4', 'Nezapomenutelné osobnosti', 11, 1, 'cz', '  ', 'en', 'The Most Unforgettable Characters', 'http://www.csfd.cz/film/68850-mash/', 'MASH 115.jpg', 115, 'Radar je přijat na "Prestižní spisovatelskou školu v Las Vegas" a popisuje v eseji své zážitky z MASHe. Frank má narozeniny a Hawkeye a B.J. předstírají hádky, aby mu udělali radost.');
INSERT INTO movie VALUES (1242, 'MASH/MASH 116.mp4', 'Suvenýry', 11, 1, 'cz', '  ', 'en', 'Souvernirs', 'http://www.csfd.cz/film/68850-mash/', 'MASH 116.jpg', 116, 'Korejské děti, ale i američtí vojáci, jsou často zranění, když se snaží sbírat různé válečné suvenýry - části výstroje nepřítele. Plukovník Potter se snaží tuto výdělečnou činnost zastavit. Hawkeye a B.J. nakonec donutí amerického dealera, aby přestal tyto věci nakupovat. Klinger, který se stále neúspěšně snaží o paragraf 8 a odvelení domů, vyleze na sloup a odmítá z něj slézt. Propuštěn sice není, ale zato překoná armádní rekord v době pobytu na sloupu.');
INSERT INTO movie VALUES (1243, 'MASH/MASH 117.mp4', '38 vodorovně', 11, 1, 'cz', '  ', 'en', '38 Across', 'http://www.csfd.cz/film/68850-mash/', 'MASH 117.jpg', 117, 'Hawkeye si neví rady s křížovkou, a tak požádá o pomoc svého dávného přítele Tippyho Brookse, odborníka na křížovky. Ten si to ale špatně vyloží a přijede do 4077 i s admirálem, protože si myslí, že ve 4077 potřebují naléhavou lékařskou výpomoc. Nakonec křížovku vyluští.');
INSERT INTO movie VALUES (1244, 'MASH/MASH 118.mp4', 'Techtle mechtle', 11, 1, 'cz', '  ', 'en', 'Hanky Panky', 'http://www.csfd.cz/film/68850-mash/', 'MASH 118.jpg', 118, 'Sestra Donovanová dostane dopis od svého manžela, který se chce rozvést, a upadne do deprese. B.J. se jí snaží pomoci a stráví s ní noc. Potm má ale pocity viny a vše se srovná až poté, co si vše na rovinu vyříkají.');
INSERT INTO movie VALUES (1245, 'MASH/MASH 119.mp4', 'Žloutenka', 11, 1, 'cz', '  ', 'en', 'Hepatitis', 'http://www.csfd.cz/film/68850-mash/', 'MASH 119.jpg', 119, 'Otec Mulcahy dostane infekční žloutenku a Hawkeye musí vyšetřit a naočkovat všechny v táboře, aby se infekce nerozšířila. Mezitím B.J. provede podle knihy obtížnou operaci a zachrání tak život těžce zraněnému vojákovi.');
INSERT INTO movie VALUES (1246, 'MASH/MASH 120.mp4', 'Dnes večer promítáme', 11, 1, 'cz', '  ', 'en', 'Movie Tonight', 'http://www.csfd.cz/film/68850-mash/', 'MASH 120.jpg', 120, 'Jako lék na vzrůstající napětí mezi osazenstvem tábora sežene plukovník Potter film ''Má drahá Clementine''. Z večera se stane kabaret, protože film je často přerušován, a tak se diváci baví jak umí. Otec Mulcahy hraje na piano, Radar předvádí své imitátorské schopnosti a každý se snaží něčím přispět k dobré náladě.');
INSERT INTO movie VALUES (1247, 'MASH/MASH 121.mp4', 'Pooperační', 11, 1, 'cz', '  ', 'en', 'Post Op', 'http://www.csfd.cz/film/68850-mash/', 'MASH 121.jpg', 121, 'Během návalu zraněných dochází ve 4077 krev. Všichni v táboře dávají v 48-hodinových intervalech krev, kromě Franka. Z krize je vytáhne až příjezd jednotky tureckých vojáků, kteří z vděku za záchranu svého velitele, darují krev.');
INSERT INTO movie VALUES (1603, '201212/Bolek a Lolek - pohadky - Popoluska.mp4', 'Popoluska', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Popoluska.jpg', 63, NULL);
INSERT INTO movie VALUES (1604, '201212/Bolek a Lolek - pohadky - Skarede kacatko.mp4', 'Skarede kacatko', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Skarede kacatko.jpg', 64, NULL);
INSERT INTO movie VALUES (1248, 'MASH/MASH 122.mp4', 'Margaretina svatba', 11, 1, 'cz', '  ', 'en', 'Margaret´s Marriage', 'http://www.csfd.cz/film/68850-mash/', 'MASH 122.jpg', 122, 'Margaret se rozhodne pod tlakem Frankových výčitek konečně stanovit datum své svatby s podplukovníkem Donaldem Penobscottem. Když Donald přijede, koná se v Bažině rozlučka se svobodou. Hawkeye s B.J. využijí Donaldovy totální opilosti a od hrudníku dolů ho dají kompletně do sádry a namluví mu, že si zlomil nohu. Nakonec se přesto obřad uskuteční a Donald s Margaret odjíždějí na svatební cestu do Tokia.');
INSERT INTO movie VALUES (1249, 'MASH/MASH 123.mp4', 'Střídání stráží', 11, 1, 'cz', '  ', 'en', 'Fade Out, Fade In', 'http://www.csfd.cz/film/68850-mash/', 'MASH 123.jpg', 123, 'Poté, co Margaret odjede na svatební cestu, upadne Frank do těžké deprese, a tak ho Potter pošle na dovolenou do Soulu. Během návalu zraněných přicházejí do 4077 zprávy o tom, že se Frank zbláznil. Potter proto žádá Tokio o dočasnou výpomoc. A tak do tábora přijíždí major Charles Emerson Winchester III. Nakonec přijde zpráva, že Frank je postupně zatčen, pak povýšen a nakonec odvelen do USA do nemocnice pro válečné veterány v Indianě. Tím pádem musí Charles zůstat v táboře natrvalo, s čímž se jen velmi těžko smiřuje.');
INSERT INTO movie VALUES (1250, 'MASH/MASH 124.mp4', 'Střídání stráží', 11, 1, 'cz', '  ', 'en', 'Fade Out, Fade In', 'http://www.csfd.cz/film/68850-mash/', 'MASH 124.jpg', 124, 'Poté, co Margareta odjede na svatební cestu, upadne Frank do těžké deprese, a tak ho Potter pošle na dovolenou do Soulu. Během návalu zraněných přicházejí do 4077 zprávy o tom, že se Frank zbláznil. Potter proto žádá Tokio o dočasnou výpomoc. A tak do tábora přijíždí major Charles Emerson Winchester III. Nakonec přijde zpráva, že Frank je postupně zatčen, pak povýšen a nakonec odvelen do USA do nemocnice pro válečné veterány v Indianě. Tím pádem musí Charles zůstat v táboře natrvalo, s čímž se jen velmi těžko smiřuje.');
INSERT INTO movie VALUES (1251, 'MASH/MASH 125.mp4', 'Kdo se směje naposled', 11, 1, 'cz', '  ', 'en', 'Last Laugh', 'http://www.csfd.cz/film/68850-mash/', 'MASH 125.jpg', 125, 'Z tábora se stává blázinec, když přijede na návštěvu starý přítel B.J. - Bardonaro, který se loučí s Koreou, protože byl odvelen do USA. Oba si vzájemně provádějí nejrůznější kanadské žertíky, ale neskončí jen u toho a obětmi se stává celá jednotka. Nakonec se ale směje Hawkeye, když pošle Bardonara na jeho cestu domů bez papírů a s minimem paliva v džípu.');
INSERT INTO movie VALUES (1252, 'MASH/MASH 126.mp4', 'Padlý idol', 11, 1, 'cz', '  ', 'en', 'Fallen Idol', 'http://www.csfd.cz/film/68850-mash/', 'MASH 126.jpg', 126, 'Na Hawkeyho návrh odjíždí Radar do Soulu, aby si užil se ženami v pověstném podniku Růžová Pagoda. Ale při cestě je ostřelován a zraněný se dostává zpátky do 4077. Hawkeyho trápí pocity viny, opije se a musí být na sále nahrazen Potterem. Když mu to Radar vyčte, pohádají se a ztratí vzájemný respekt. Ale vše se nakonec urovná, když Radar dostane Purpurové srdce a sám Hawkeye mu ho připne a udělá něco, co nikdy neudělal - předpisově mu zasalutuje.');
INSERT INTO movie VALUES (1253, 'MASH/MASH 127.mp4', 'Tetovaný', 11, 1, 'cz', '  ', 'en', 'Images', 'http://www.csfd.cz/film/68850-mash/', 'MASH 127.jpg', 127, 'Radarovi se velmi zalíbí tetování, které má jeden zraněný voják a rozhodne se také si nějaké tetování pořídit, aby se stal pro ženy atraktivnější. Všichni se mu to snaží rozmluvit, ale Radar nakonec stejně odejde do tetovacího salonu. Teprve po několika dnech Hawkeye a B.J. zjistí, že si nechal místo tetování udělat jen obrázek, který jde smýt vodou.');
INSERT INTO movie VALUES (1254, 'MASH/MASH 128.mp4', 'Válka nervů', 11, 1, 'cz', '  ', 'en', 'War of Nerves', 'http://www.csfd.cz/film/68850-mash/', 'MASH 128.jpg', 128, 'Jednotka prožívá těžkou psychickou zátěž, když po několik dní nepřetržitě přicházejí ranění. Atmosféra houstne a do tábora přijíždí psychiatr Sidney Freedman, sám ve špatném stavu, protože ho jeden z pacientu viní ze svého zranění. Sidney vymýšlí, jak zlepšit špatnou náladu, ale nakonec se problém vyřeší pozoruhodnou terapií - uprostřed tábora je rozdělán velký oheň, kam si každý něco přihodí, aby si tak ulevil.');
INSERT INTO movie VALUES (1255, 'MASH/MASH 129.mp4', 'Winchesterovy nahrávky', 11, 1, 'cz', '  ', 'en', 'The Winchester Tapes', 'http://www.csfd.cz/film/68850-mash/', 'MASH 129.jpg', 129, 'Hawkeye se neúspěšně snaží odjet do Tokia, kde má strávit víkend s jednou sestrou. Mezitím Charles nahrává na pásku svůj dopis domů, ve kterém žádá své vlivné rodiče, aby ho dostali zpět do Států. Hawkeye s B.J. mu jen tak pro zábavu stále mění kalhoty, takže si Charles nejprve myslí, že hubne, a tak se začne přejídat a posléze zjistí, že naopak ztloustl.');
INSERT INTO movie VALUES (1256, 'MASH/MASH 130.mp4', 'Světlo, které zhaslo', 11, 1, 'cz', '  ', 'en', 'The Light That Failed', 'http://www.csfd.cz/film/68850-mash/', 'MASH 130.jpg', 130, 'Ve 4077 docházejí zásoby a místo nových, přiváží nákladní auto stroje na zmrzlinu a slané tablety. B.J. dostává poštou detektivku, kterou po částech čtou snad všichni v táboře. Ale poslední stránka se ztratí, a tak záhada zůstává nedořešena, dokud B.J. s Hawkeyem nezavolají do Států autorce knihy.');
INSERT INTO movie VALUES (1257, 'MASH/MASH 131.mp4', 'Čaj', 11, 1, 'cz', '  ', 'en', 'Tea and Empathy', 'http://www.csfd.cz/film/68850-mash/', 'MASH 131.jpg', 131, 'Do tábora proudí velké množství Britských a Amerických raněných a někdo ukradne všechny zásoby penicilínu, čímž jsou ohroženy životy zraněných. Otec Mulcahy zjistí, kde by mohl nějaký penicilín být a spolu s Klingerem se pro něj vydají. Přesto, že jsou ostřelováni, penicilín najdou a dovezou do tábora.');
INSERT INTO movie VALUES (1258, 'MASH/MASH 132.mp4', 'Hrobař', 11, 1, 'cz', '  ', 'en', 'The Grim Reaper', 'http://www.csfd.cz/film/68850-mash/', 'MASH 132.jpg', 132, 'Plukovník Bloodworth předpoví před bitvou, že do tábora dorazí 280 raněných. Hawkeye se rozzuří nad jeho cynismem a dostane se s ním do konfliktu. Plukovník na Hawkeyho podá žalobu, kterou stáhne poté, co se sám dostane do tábora jako raněný, vidí Hawkeyeho při práci a pochopí, pod jakým tlakem je.');
INSERT INTO movie VALUES (1259, 'MASH/MASH 133.mp4', 'Olympiáda v M*A*S*Hi', 11, 1, 'cz', '  ', 'en', 'The M*A*S*H Olympics', 'http://www.csfd.cz/film/68850-mash/', 'MASH 133.jpg', 133, 'Plukovník Potter přemýšlí, jak zvýšit tělesnou zdatnost osazenstva MASHe, a rozhodne se uspořádat olympiádu. Vytvoří se dvě družstva - pod vedením Hawkeyho a B.J. První cena je dovolená v Tokiu, takže se všichni velmi snaží. Nakonec zvítězí Hawkeyho družstvo a B.J. musí celý týden Hawkeyho vozit v invalidním vozíku.');
INSERT INTO movie VALUES (1260, 'MASH/MASH 134.mp4', 'Láska a válka', 11, 1, 'cz', '  ', 'en', 'In Love and War', 'http://www.csfd.cz/film/68850-mash/', 'MASH 134.jpg', 134, 'Hawkeye se zamiluje do Kyong Soon - korejské ženy, která pečuje o svou nemocnou matku a o děti. Ale všechny naděje jsou zmařeny, když matka zemře a Kyong musí s dětmi odejít na jih na bezpečnější místo.');
INSERT INTO movie VALUES (1261, 'MASH/MASH 135.mp4', 'Výměnný den', 11, 1, 'cz', '  ', 'en', 'Change Day', 'http://www.csfd.cz/film/68850-mash/', 'MASH 135.jpg', 135, 'Charles chce zbohatnout na plánované výměně armádních peněz za nové a vykupuje hluboko pod cenou staré bankovky od místních Korejců. Hawkeye s B.J. ho ale odhalí a překazí mu jeho akci, takže mu nakonec zbude jen hrst neplatných papírů.');
INSERT INTO movie VALUES (1605, '201212/Bolek a Lolek - pohadky - Snehova kralovka.mp4', 'Snehova kralovna', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Snehova kralovka.jpg', 65, NULL);
INSERT INTO movie VALUES (1606, '201212/Bolek a Lolek - pohadky - Svaja rytieri.mp4', 'Dvaja rytieri', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Svaja rytieri.jpg', 66, NULL);
INSERT INTO movie VALUES (1262, 'MASH/MASH 136.mp4', 'Pacient 4077', 11, 1, 'cz', '  ', 'en', 'Patient 4077', 'http://www.csfd.cz/film/68850-mash/', 'MASH 136.jpg', 136, 'Hawkeye a B.J. potřebují nutně nový chirurgický nástroj, který ale není nikdo schopen vyrobit. Až místní prodavač hodinek Mr. Shin to dokáže a hned za pár dní s ním v táboře zachrání nohu zraněného vojáka. Pan Shin začne poté pracovat jako výrobce chirurgických nástrojů.');
INSERT INTO movie VALUES (1263, 'MASH/MASH 137.mp4', '''Vůně'' hudby', 11, 1, 'cz', '  ', 'en', 'The Smell of Music', 'http://www.csfd.cz/film/68850-mash/', 'MASH 137.jpg', 137, 'Charles se vyžívá ve hře na roh, z čehož se můžou Hawkeye a B.J. zbláznit. Na protest se přestanou umývat, dokud Charles neskončí s hraním. Nakonec je pro všechny situace neudržitelná, a tak kolektivně chytí Hawkeye a B.J. a umyjí je a přes Charlesův roh přejede džíp.');
INSERT INTO movie VALUES (1264, 'MASH/MASH 138.mp4', 'Kamarádi ve zbrani', 11, 1, 'cz', '  ', 'en', '1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 138.jpg', 138, 'Hawkeye a Margaret se vydávají na výpomoc do jednotky 8063, ale ztratí se za nepřátelskou linii. Musí se schovat na noc do opuštěné chatrče, kde se během bombardování velmi sblíží...');
INSERT INTO movie VALUES (1265, 'MASH/MASH 139.mp4', 'Kamarádi ve zbrani', 11, 1, 'cz', '  ', 'en', '2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 139.jpg', 139, 'Ráno po probuzení Margaret projevuje Hawkeymu velkou náklonnost, ale jen do té doby, než se stihnou znovu pohádat. B.J. se je vydá vrtulníkem hledat a nakonec je objeví.');
INSERT INTO movie VALUES (1266, 'MASH/MASH 140.mp4', 'Kupec Korejský', 11, 1, 'cz', '  ', 'en', 'The Merchant of Korea', 'http://www.csfd.cz/film/68850-mash/', 'MASH 140.jpg', 140, 'Charles půjčí B.J. dvě stě dolarů, a poté toho začne využívat. Ostatní se domluví, že ho obehrají v pokeru. Ale Charles má velké štěstí a postupně získává víc a víc peněz. Naštěstí si Radar všimne, že Charles, vždy když blafuje, píská. Všichni vyhrají své peníze zpět a každý i něco navíc.');
INSERT INTO movie VALUES (1267, 'MASH/MASH 141.mp4', 'O co jde, šéfe?', 11, 1, 'cz', '  ', 'en', 'What´s Up, Doc?', 'http://www.csfd.cz/film/68850-mash/', 'MASH 141.jpg', 141, 'Margareta si myslí, že je těhotná a chce, aby ji Hawkeye udělal test, ale k tomu potřebuje vaječníky Radarova králíka. Radar ho půjčí jen pod podmínkou, že ho Hawkeye nezabije. Mezitím se jeden pacient snaží dostat domů a se zbraní drží jako rukojmí Charlese a Klingera. Nakonec ale z nedostatku krve omdlí a vše se vyřeší. Margareta není těhotná, ale sama neví, jestli je to dobře.');
INSERT INTO movie VALUES (1268, 'MASH/MASH 142.mp4', 'Potterovo odvolání', 11, 1, 'cz', '  ', 'en', 'Potter´s Retirement', 'http://www.csfd.cz/film/68850-mash/', 'MASH 142.jpg', 142, 'Potter je povolán ke generálu Waldo Kentovi, který mu sdělí, že zevnitř 4077 přicházejí stížnosti na jeho velení. Potter je znechucen a chystá se odejít do Států. Nakonec se ukáže, že dopisy psal desátník Benson, který byl k tomu účelu vyslán do 4077 pomateným plukovníkem Frankem Websterem, který nemocnicí nedávno prošel.');
INSERT INTO movie VALUES (1269, 'MASH/MASH 143.mp4', 'Volaní pošty potřetí', 11, 1, 'cz', '  ', 'en', 'Mail Call Three', 'http://www.csfd.cz/film/68850-mash/', 'MASH 143.jpg', 143, 'Po třech týdnech čekání konečně dorazí pošta a každý v táboře reaguje na dobré i špatné zprávy z domova. Hawkeye dostává milostné dopisy, které ale patří jeho jmenovci Benjaminu Pierceovi, B.J.ova žena píše, že byla obtěžována jiným mužem a Radarova matka si našla přítele.');
INSERT INTO movie VALUES (1270, 'MASH/MASH 144.mp4', 'Dr. Winchester a Mr. Hyde', 11, 1, 'cz', '  ', 'en', 'Dr. Winchester and Mr. Hyde', 'http://www.csfd.cz/film/68850-mash/', 'MASH 144.jpg', 144, 'Charles nemůže vydržet tempo práce a začne brát amfetamin, který zvýší jeho výkon. Namlouvá si, že má vše pod kontrolou, ale nakonec zkolabuje a Hawkeye a B.J. ho odhalí. Mezitím nadopovaná Radarova myš Daisy porazí v závodech konkurenční myš námořnictva.');
INSERT INTO movie VALUES (1271, 'MASH/MASH 145.mp4', 'Major Machr', 11, 1, 'cz', '  ', 'en', 'Major Toppo', 'http://www.csfd.cz/film/68850-mash/', 'MASH 145.jpg', 145, 'V táboře dochází morfium, a tak se plukovník Potter rozhodne vyzkoušet placebo - tablety z cukru, které vydávají za účinný lék proti bolesti a nejméně v 50 procentech to pomůže. Mezitím major Winchester ohromuje všechny svými historkami o tom, co všechno zažil.');
INSERT INTO movie VALUES (1272, 'MASH/MASH 146.mp4', 'Hitparáda', 11, 1, 'cz', '  ', 'en', 'Your Hit Parade', 'http://www.csfd.cz/film/68850-mash/', 'MASH 146.jpg', 146, 'Do tábora dorazí zásilka s deskami a Radar se stane diskžokejem. Pomocí táborového rozhlasu pouští hudbu a pomáhá tak zlepšovat náladu během dlouhé služby.');
INSERT INTO movie VALUES (1273, 'MASH/MASH 147.mp4', 'Dočasná výměna', 11, 1, 'cz', '  ', 'en', 'Temporary Duty', 'http://www.csfd.cz/film/68850-mash/', 'MASH 147.jpg', 147, 'Hawkeye je na týden převelen na výměnu zkušeností do 8063 a místo něho přichází kapitán Roy Dupree. Dupree se snaží prosadit, aby výměna byla natrvalo, ale B.J. s Charlesem naštěstí zařídí, aby byl navždy odvelen z 4077 pryč.');
INSERT INTO movie VALUES (1274, 'MASH/MASH 148.mp4', 'Mír s námi', 11, 1, 'cz', '  ', 'en', 'Peace On Us', 'http://www.csfd.cz/film/68850-mash/', 'MASH 148.jpg', 148, 'Hawkeye se velmi rozčílí, když přijdou zprávy, že mírová jednání v Panmudžonu nevedou nikam. Rozhodne se tam dostat a promluvit si s delegáty. To se mu na konec také podaří - válku sice neukončí, ale celá jednotka na jeho počest uspořádá velkou oslavu.');
INSERT INTO movie VALUES (1275, 'MASH/MASH 149.mp4', 'B.J. Papa San', 11, 1, 'cz', '  ', 'en', 'B.J.Papa San', 'http://www.csfd.cz/film/68850-mash/', 'MASH 149.jpg', 149, 'B.J. se snaží nahradit nemocného otce jedné korejské rodině. Nachází v ní náhradu za svou vlastní rodinu a tráví s ní mnoho času, a snaží se všemožně pomoci. Tato činnost ho ale stojí mnoho sil a Hawkeye se začne obávat o jeho zdraví. Nakonec se ale Korejci odstěhují ze strachu před bombardováním neznámo kam.');
INSERT INTO movie VALUES (1276, 'MASH/MASH 150.mp4', 'Hej, je tu zima!', 11, 1, 'cz', '  ', 'en', 'Baby, it´s Cold Outside', 'http://www.csfd.cz/film/68850-mash/', 'MASH 150.jpg', 150, 'V táboře udeří krutá zima a navíc je málo teplého oblečení. Charles dostane z domova od rodičů zimní bundu a dává všem najevo, jaké je mu teplo, čímž se stane nejméně populární osobou v táboře.');
INSERT INTO movie VALUES (1277, 'MASH/MASH 151.mp4', 'Velitel Pierce', 11, 1, 'cz', '  ', 'en', 'Commander Pierce', 'http://www.csfd.cz/film/68850-mash/', 'MASH 151.jpg', 151, 'Hawkeye projde drastickou proměnou, když se stane dočasným velitelem jednotky za plukovníka Pottera, který musí do Tokia. Hawkeye se střetává s armádní byrokracií a poznává, kolik problémů musí Potter každodenně řešit.');
INSERT INTO movie VALUES (1278, 'MASH/MASH 152.mp4', 'Billfoldův syndrom', 11, 1, 'cz', '  ', 'en', 'The Billfold Syndrome', 'http://www.csfd.cz/film/68850-mash/', 'MASH 152.jpg', 152, 'Charles dostává z domova špatné zprávy ohledně své budoucí lékařské kariéry a odmítá mluvit s kýmkoliv z jednotky. Nakonec mu Hawkeye s B.J. pošlou falešný telegram z domova. Mezitím si mladý voják nemůže vzpomenout na vlastní identitu a tak je na pomoc povolán Sidney Freedman.');
INSERT INTO movie VALUES (1279, 'MASH/MASH 153.mp4', 'Lil', 11, 1, 'cz', '  ', 'en', 'Lil', 'http://www.csfd.cz/film/68850-mash/', 'MASH 153.jpg', 153, 'Do tábora přijíždí na inspekci armádní vrchní sestra - plukovnice Lil Rayburnová. Plukovník Potter s ní naváže velmi vřelý, přátelský vztah, protože je to po dlouhé době osoba jeho věku, se kterou se cítí velmi dobře. Radar má podezření, že za jejich vztahem je víc než přátelství a snaží se tomu zabránit.');
INSERT INTO movie VALUES (1280, 'MASH/MASH 154.mp4', 'Volaní korejského větru', 11, 1, 'cz', '  ', 'en', 'They Call the Wind Korea', 'http://www.csfd.cz/film/68850-mash/', 'MASH 154.jpg', 154, 'Tábor je varován, že se blíží silná bouře a všichni se na ní chystají. Hawkeye a ostatní se snaží zajistit tábor a pacienty proti větru a zimě, Radar připravuje svoje zvířata na nejhorší. Charles nemůže odletět vrtulníkem na dovolenou do Tokia a proto jede s Klingerem džípem. Cestou ale zabloudí a narazí na 4 zraněné řecké vojáky, které ošetří a přečkají s nimi bouři. Ráno zjistí, že byli 200 metrů od tábora.');
INSERT INTO movie VALUES (1281, 'MASH/MASH 155.mp4', 'Naše nejlepší okamžiky 1', 11, 1, 'cz', '  ', 'en', 'Our Finest Hour 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 155.jpg', 155, 'Do 4077 přijíždí válečný zpravodaj Clete Roberts, aby udělal interview se všemi významnými členy MASHe. Tyto černobílé rozhovory se střídají s nejlepšími ukázkami ze starších dílů.');
INSERT INTO movie VALUES (1282, 'MASH/MASH 156.mp4', 'Naše nejlepší okamžiky 2', 11, 1, 'cz', '  ', 'en', 'Our Finest Hour 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 156.jpg', 156, 'Do 4077 přijíždí válečný zpravodaj Clete Roberts, aby udělal interview se všemi významnými členy MASHe. Tyto černobílé rozhovory se střídají s nejlepšími ukázkami ze starších dílů.');
INSERT INTO movie VALUES (1283, 'MASH/MASH 157.mp4', 'Nikdo to nemá rád horké', 11, 1, 'cz', '  ', 'en', 'None Like it Hot', 'http://www.csfd.cz/film/68850-mash/', 'MASH 157.jpg', 157, 'Velké vedro sužuje každého v táboře, ale asi nejvíc Klingera, který se snaží dostat domů tentokrát tím, že v horku na sobě nosí co nejvíce šatů. Mezitím Hawkeye a B.J. dostanou skládací vanu a snaží se to udržet v tajnosti, což se jim nepodaří, a tak vanu používá celý tábor. Nakonec se Hawkeye s B.J. rozhodnou vanu vyměnit u překupníka za 10 litru zmrzliny pro Radara, který je po operaci mandlí.');
INSERT INTO movie VALUES (1284, 'MASH/MASH 158.mp4', 'Bez plynu', 11, 1, 'cz', '  ', 'en', 'Out of Glas', 'http://www.csfd.cz/film/68850-mash/', 'MASH 158.jpg', 158, 'Do tábora přivážejí velmi mnoho raněných, což působí velké problémy, protože dochází uspávací plyn pentothal, a tak musí uspávat pacienty klasicky - éterem. Otec Mulcahy se rozhodne, že pentothal získá od překupníků, a proto uspořádá sbírku. Nakonec se mu po problémech s Charlesem přesto podaří několik beden petnothalu získat.');
INSERT INTO movie VALUES (1285, 'MASH/MASH 159.mp4', 'Major Ego', 11, 1, 'cz', '  ', 'en', 'Major Ego', 'http://www.csfd.cz/film/68850-mash/', 'MASH 159.jpg', 159, 'Charles je oslavován jako hrdina poté, co zachrání umírajícího pacienta pomocí masáže srdce. Dokonce o něm má vyjít i článek v časopisu ''Hvězdy a pruhy''. Nakonec ale žádný článek nevyjde, protože si Charles uvědomí, že není tak dobrý, jak si myslel.');
INSERT INTO movie VALUES (1286, 'MASH/MASH 160.mp4', 'Drahý Soudruhu', 11, 1, 'cz', '  ', 'en', 'Dear Comrade', 'http://www.csfd.cz/film/68850-mash/', 'MASH 160.jpg', 160, 'Charles si chce zpříjemnit svůj pobyt v Koreji, a tak si najme sluhu. Ten je ale ve skutečnosti severokorejským špionem a má za úkol zjistit, co se skrývá za velkými úspěchy 4077. Nakonec zjistí, že styl jejich práce je natolik odlišný, že by se nedal na severu použít. Mezitím ještě stačí pomoci s léčbou neznámé vyrážky, za což dostane vyznamenání armády Spojených států.');
INSERT INTO movie VALUES (1287, 'MASH/MASH 161.mp4', 'Oko za oko, zub za zub', 11, 1, 'cz', '  ', 'en', 'An Eye for a Tooth', 'http://www.csfd.cz/film/68850-mash/', 'MASH 161.jpg', 161, 'Otec Mulcahy zjistí, že opět není na seznamu mezi povýšenými a rozhodne se vykonat hrdinský čin. Pilot helikoptéry musí letět na frontu pro zraněného vojáka a Mulcahy letí s ním, jako zátěž na vyvážení vrtulníku. Mezitím si Hawkeye s B.J. a Margaretou vzájemně tropí různé kanadské žertíky než přijdou na to, že v pozadí všeho stojí Charles.');
INSERT INTO movie VALUES (1288, 'MASH/MASH 162.mp4', 'Na vlastní oči', 11, 1, 'cz', '  ', 'en', 'Point of View', 'http://www.csfd.cz/film/68850-mash/', 'MASH 162.jpg', 162, 'Tato unikátní epizoda je natočena jakoby z pohledu zraněného vojáka. Začíná zraněním na frontě, poté následuje převoz vrtulníkem, předoperační, operace a pobyt na pooperačním až po odvoz domů.');
INSERT INTO movie VALUES (1289, 'MASH/MASH 163.mp4', 'Preventivní léčba', 11, 1, 'cz', '  ', 'en', 'Preventive Medicine', 'http://www.csfd.cz/film/68850-mash/', 'MASH 163.jpg', 163, 'Do tábora přijíždí plukovník Lacy, jehož jednotka má díky jeho nezodpovědnosti a touze po slávě nejvyšší procento raněných. Lacy se chystá za cenu velkých ztrát dobýt nové území a Hawkeye se mu v tom snaží zabránit. Nakonec mu přes B.J.ovy protesty vyoperuje slepé střevo, čímž ho na několik dnů vyřadí z boje.');
INSERT INTO movie VALUES (1290, 'MASH/MASH 164.mp4', 'Drahá Sestro', 11, 1, 'cz', '  ', 'en', 'Dear Sis', 'http://www.csfd.cz/film/68850-mash/', 'MASH 164.jpg', 164, 'Otec Mulcahy píše dopis k Vánocům své sestře, která je jeptiška. Trápí ho pocit, že je v táboře málo užitečný a že by se měl zlepšit. Špatná nálada ale doléhá na všechny, protože musejí trávit Vánoce daleko od svých blízkých.');
INSERT INTO movie VALUES (1291, 'MASH/MASH 165.mp4', 'Cena', 11, 1, 'cz', '  ', 'en', 'The Price', 'http://www.csfd.cz/film/68850-mash/', 'MASH 165.jpg', 165, 'Do života 4077 zasáhnou dvě mimořádné situace. Potterův kůň záhadně zmizí z ohrady a Hawkeye s B.J. se snaží zachránit mladého Korejce před odvedením do armády. Nakonec se kůň najde a mladý Korejec odejde do války dobrovolně.');
INSERT INTO movie VALUES (1292, 'MASH/MASH 166.mp4', 'Šťabajzna se vrací', 11, 1, 'cz', '  ', 'en', 'Hot Lips is Back in Town', 'http://www.csfd.cz/film/68850-mash/', 'MASH 166.jpg', 166, 'Radar se zamiluje do nové roztomilé sestry Lindy Nugentové a snaží se ji zaujmout podle různých rad od Hawkeyho a Charlese. Nakonec ale pochopí, že v podobných situacích je nejlepší zůstat sám sebou. Mezitím se Margareta vyrovnává se svým úředně potvrzeným rozvodem.');
INSERT INTO movie VALUES (1293, 'MASH/MASH 167.mp4', 'Inga', 11, 1, 'cz', '  ', 'en', 'Inga', 'http://www.csfd.cz/film/68850-mash/', 'MASH 167.jpg', 167, 'Do tábora přijíždí půvabná švédská doktorka studovat válečnou medicínu a Hawkeye se ji snaží svést. Ale poté, co doktorka na sále prokáže lepší techniku než on sám, musí se vyrovnávat se svým pošramoceným sebevědomím.');
INSERT INTO movie VALUES (1294, 'MASH/MASH 168.mp4', 'Mladí a neklidní', 11, 1, 'cz', '  ', 'en', 'The Young and Restless', 'http://www.csfd.cz/film/68850-mash/', 'MASH 168.jpg', 168, 'Přednáška o nejnovějších chirurgických technikách od mladého doktora z Tokia a jejich pozdější demonstrace přímo na sále vede k tomu, že chirurgové z MASHe si uvědomí, jak moc zaostávají, když nejsou v kontaktu s moderní medicínou.');
INSERT INTO movie VALUES (1295, 'MASH/MASH 169.mp4', 'Ach ta láska', 11, 1, 'cz', '  ', 'en', 'Ain´t Love Grand', 'http://www.csfd.cz/film/68850-mash/', 'MASH 169.jpg', 169, 'Stane se něco opravdu výjimečného - snobský Charles se zamiluje do korejské prostitutky Sooni a snaží se ji vychovávat podle sebe. Ta to ale nakonec nevydrží a vrátí se ke svému řemeslu. Mezitím prožije milostný románek i Klinger s americkou sestrou Debbie. Nakonec se Klinger s Charlesem coby zrazení milenci společně opijí u Rosie v baru a zpívají spolu smutné písně.');
INSERT INTO movie VALUES (1296, 'MASH/MASH 170.mp4', 'J*E*S*K*Y*N*Ě', 11, 1, 'cz', '  ', 'en', 'C*A*V*E', 'http://www.csfd.cz/film/68850-mash/', 'MASH 170.jpg', 170, 'Tábor je bohužel blízko oblasti, která má být bombardována a tak se musí dočasně přesunout do blízké jeskyně, což způsobí velké problémy Hawkeymu, který trpí klaustrofóbií. Margareta zase nesnáší velký hluk a navíc je tu ještě těžce zraněný voják...');
INSERT INTO movie VALUES (1297, 'MASH/MASH 171.mp4', 'Párty', 11, 1, 'cz', '  ', 'en', 'The Party', 'http://www.csfd.cz/film/68850-mash/', 'MASH 171.jpg', 171, 'B.J. dostane nápad uspořádat ve Státech setkání rodičů, manželek a příbuzných všech příslušníků 4077. Zpočátku této myšlence nikdo nedůvěřuje, ale nakonec B.J. všechny přesvědčí, setkání se opravdu uskuteční a velmi se vydaří.');
INSERT INTO movie VALUES (1298, 'MASH/MASH 172.mp4', 'Pozor na Flagga, chlapci!', 11, 1, 'cz', '  ', 'en', 'Rally Round the Flagg, Boys', 'http://www.csfd.cz/film/68850-mash/', 'MASH 172.jpg', 172, 'Bláznivý plukovník Flagg se náhle zjeví v táboře a začne spřádat své špiónské plány, protože podezřívá Hawkeyho ze spolupráce s komunisty. Chce zlákat ke spolupráci i Charlese, ale ten ho nakonec dokonale napálí.');
INSERT INTO movie VALUES (1299, 'MASH/MASH 173.mp4', 'Noc u Rosie', 11, 1, 'cz', '  ', 'en', 'A Night at Rosie´s', 'http://www.csfd.cz/film/68850-mash/', 'MASH 173.jpg', 173, 'Hawkeye, B.J. a ostatní osazenstvo 4077 objeví novou cestu k útěku před depresivní atmosférou války a zůstanou v baru u Rosie, který nemíní opustit přes velkou nelibost plukovníka Pottera.');
INSERT INTO movie VALUES (1300, 'MASH/MASH 174.mp4', 'Příliš mnoho kuchařů', 11, 1, 'cz', '  ', 'en', 'S-601', 'http://www.csfd.cz/film/68850-mash/', 'MASH 174.jpg', 174, 'Nemotorný voják, který leží na pooperačním si získá velikou oblibu v MASHi svým kuchařským uměním. Všichni si jeho gurmánské speciality velmi oblíbí a Hawkeye s B.J. se ho snaží získat pro 4077. To se ale nepodaří, protože voják se chce vrátit na frontu a vařit pro své kamarády. Mezitím prožívá menší krizi plukovník Potter, když dostane špatný dopis od manželky, ale nakonec se vše urovná. Pozn.: V této epizodě se objevuje Gary Burghoff (Radar) již jen jako host.');
INSERT INTO movie VALUES (1301, 'MASH/MASH 175.mp4', 'Kdo jsi, Margaret?', 11, 1, 'cz', '  ', 'en', 'Are You Now, Margaret', 'http://www.csfd.cz/film/68850-mash/', 'MASH 175.jpg', 175, 'Tábor navštíví v rámci cesty po Koreji Williamson, poradce kongresmana. Původně to má být jen rutinní návštěva, ale ukazuje se, že motivy jsou hlubší - Margareta je podezřelá ze spolupráce s komunisty.');
INSERT INTO movie VALUES (1302, 'MASH/MASH 176.mp4', 'Partyzánské sny', 11, 1, 'cz', '  ', 'en', 'Guerilla My Dreams', 'http://www.csfd.cz/film/68850-mash/', 'MASH 176.jpg', 176, 'Příjezd zraněné Korejky způsobí v táboře konflikt - Hawkeye se ji snaží vyléčit, ale přísný korejský důstojník se nejvíce zajímá o to, jak z ní dostat informace o její partyzánské činnosti.');
INSERT INTO movie VALUES (1303, 'MASH/MASH 177.mp4', 'Sbohem Radare 1', 11, 1, 'cz', '  ', 'en', 'Good-Bye Radar 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 177.jpg', 177, 'Taborový úředník Radar O''Reilly je na dovolené v Tokiu, ale v krizi zmítaném 4077 ho nutně potřebují. Jeho návrat se ale stále vlivem různých okolností odkládá. Navíc přestane v táboře fungovat generátor, čímž se ocitnou všechny přístroje bez proudu. A tak se Klinger, který zastupuje Radara, snaží sehnat nový.');
INSERT INTO movie VALUES (1304, 'MASH/MASH 178.mp4', 'Sbohem Radare 2', 11, 1, 'cz', '  ', 'en', 'Good-Bye Radar 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 178.jpg', 178, 'Radar se dozvídá o smrti svého strýčka, a tak, ač nerad opouští 4077 a odjíždí domů do Států. Mezitím je tábor stále bez proudu a stále přicházejí další zranění. Odpovědnost za sehnání nového generátoru leží na Klingerovi, který ale zatím postrádá Radarovy zkušenosti v');
INSERT INTO movie VALUES (1305, 'MASH/MASH 179.mp4', 'Čas přizpůsobování', 11, 1, 'cz', '  ', 'en', 'Period of Adjustment', 'http://www.csfd.cz/film/68850-mash/', 'MASH 179.jpg', 179, 'Klinger, který se stal po odchodu Radara novým');
INSERT INTO movie VALUES (1306, 'MASH/MASH 180.mp4', 'Osobní finance', 11, 1, 'cz', '  ', 'en', 'Private Finance', 'http://www.csfd.cz/film/68850-mash/', 'MASH 180.jpg', 180, 'Jihokorejská žena si špatně vysvětluje Klingerovy úmysly, když se snaží finančně vypomoci její dceři. Mezitím Hawkeye bojuje se svým svědomím kvůli slibu, který dal umírajícímu vojákovi.');
INSERT INTO movie VALUES (1307, 'MASH/MASH 181.mp4', 'Pan a paní Kdo?', 11, 1, 'cz', '  ', 'en', 'Mr. Mrs. Who?', 'http://www.csfd.cz/film/68850-mash/', 'MASH 181.jpg', 181, 'Charles Winchester se vrací do 4077 po dovolené v Tokiu otřesen milostnou aférou. Mezitím se nemocnice snaží sehnat léky proti záchvatům smrtelné krvácející zimnice.');
INSERT INTO movie VALUES (1308, 'MASH/MASH 182.mp4', 'Cesta ke žlutému mostu', 11, 1, 'cz', '  ', 'en', 'The Yalu Brick Road', 'http://www.csfd.cz/film/68850-mash/', 'MASH 182.jpg', 182, 'Hawkeye a B.J. se ztratí, když pospíchají s');
INSERT INTO movie VALUES (1309, 'MASH/MASH 183.mp4', 'Sestra doktorka', 11, 1, 'cz', '  ', 'en', 'Nurse Doctor', 'http://www.csfd.cz/film/68850-mash/', 'MASH 183.jpg', 183, 'Pěkná a ambiciózní mladá sestra, která se chce stát po opuštění armády doktorkou, se dostává do konfliktu s některými obyvateli MASHe, ale nakonec se vše urovná. Mezitím docházejí v táboře zásoby vody, a tak osazenstvo tábora trpí nekvalitním jídlem a nefunkční sprchou.');
INSERT INTO movie VALUES (1310, 'MASH/MASH 184.mp4', 'Čas života', 11, 1, 'cz', '  ', 'en', 'Life Time', 'http://www.csfd.cz/film/68850-mash/', 'MASH 184.jpg', 184, 'Do 4077 je přivezen vrtulníkem k okamžitému ošetření těžce zraněný voják, který zemře nebo bude ochrnutý, pokud nedostane transplantovanou aortu do 20 minut. Celý boj o jeho záchranu je znázorněn minutu po minutě. Pozn.: O to, aby tento díl byl do posledního detailu z pohledu lékařů správný, se postaral lékařský konzultant seriálu Walter Dishell.');
INSERT INTO movie VALUES (1311, 'MASH/MASH 185.mp4', 'Sny', 11, 1, 'cz', '  ', 'en', 'Dreams', 'http://www.csfd.cz/film/68850-mash/', 'MASH 185.jpg', 185, 'Nikdo z tábora nemůže utéci před korejskou válkou, kromě svých snů. Po dvou dnech na sále jsou všichni unaveni a jeden po druhem usínají a zdají se jim sny, ve kterých se odráží jejich strach, touhy a pocity marnosti.');
INSERT INTO movie VALUES (1312, 'MASH/MASH 186.mp4', 'Drahý strýčku Abdule', 11, 1, 'cz', '  ', 'en', 'Dear Uncle Abdul', 'http://www.csfd.cz/film/68850-mash/', 'MASH 186.jpg', 186, 'Klinger zjišťuje, že jeho služba jako úředník jednotky obsahuje také plnění různých bláznivých výmyslů jeho nadřízených - musí stát modelem pro plukovníkův obraz, musí chodit s majorem Winchestrem na lov kachen a podobně. Mezitím si najde alespoň chvilku času na to, aby napsal domu dopis, ve kterém zjišťuje, že vůbec není největším bláznem jednotky.');
INSERT INTO movie VALUES (1313, 'MASH/MASH 187.mp4', 'Bitva o kapitána', 11, 1, 'cz', '  ', 'en', 'Captain´s Outrageous', 'http://www.csfd.cz/film/68850-mash/', 'MASH 187.jpg', 187, 'V baru u Rosie se strhne bitka a Rosie musí být hospitalizována se zlomenými žebry, a tak za ní doktoři zaskakují v baru. Mezitím se otec Mulcahy dozvídá, že jeho povýšení na kapitána je opět odloženo, ale nakonec se vše vyřeší a Mulcahy je povýšen.');
INSERT INTO movie VALUES (1314, 'MASH/MASH 188.mp4', 'Hvězdy a pruhy', 11, 1, 'cz', '  ', 'en', 'Stars and Stripes', 'http://www.csfd.cz/film/68850-mash/', 'MASH 188.jpg', 188, 'Mezi B.J. a Winchesterem nastanou třenice poté, co jsou požádáni, aby společně napsali článek do prestižního doktorského časopisu o tom, jak zachránili náročnou operací život jednoho vojáka. Mezitím přijíždí na návštěvu za Margaretou její starý přítel Scully a jeho návštěva se neobejde bez hádek, ale nakonec se přece jen usmíří.');
INSERT INTO movie VALUES (1315, 'MASH/MASH 189.mp4', 'Uzdravte je', 11, 1, 'cz', '  ', 'en', 'Heal Thyself', 'http://www.csfd.cz/film/68850-mash/', 'MASH 189.jpg', 189, 'Plukovník Potter musí ulehnout, když dostane příušnice. Jeho nálada se ještě zhorší, když k sobě dostane dalšího nájemníka se stejnou nemoci - Charlese Winchestera. Do 4077 přijíždí dočasně náhradní chirurg, který je oba výborně nahradí, ale jen do té doby, než neunese tíhu situace a zhroutí se.');
INSERT INTO movie VALUES (1316, 'MASH/MASH 190.mp4', 'Ano, to je naše dítě', 11, 1, 'cz', '  ', 'en', 'Yessir, That´s Our Baby', 'http://www.csfd.cz/film/68850-mash/', 'MASH 190.jpg', 190, 'V táboře se objeví odložené dítě korejské matky a amerického vojáka. Všichni se o něj starají, ale vědí, že tyto děti nemají v korejské komunitě mnoho šancí na přežití. Proto se ho snaží dostat do Států. To se nakonec nepodaří, tak ho alespoň umístí v jednom místním klášteře.');
INSERT INTO movie VALUES (1317, 'MASH/MASH 191.mp4', 'Vyčerpaná láhev', 11, 1, 'cz', '  ', 'en', 'Bottle Fatigue', 'http://www.csfd.cz/film/68850-mash/', 'MASH 191.jpg', 191, 'Hawkeye se zhrozí, když zjistí výši svého měsíčního dluhu v baru v důstojnickém klubu a slíbí, že se týden nedotkne alkoholu. Nakonec to vydrží, i když všem svým chováním leze na nervy. Mezitím se Charles snaží zabránit sňatku své sestry s italským přistěhovalcem. Pozn.: V této epizodě se poprvé objevuje Shari Sabaová, která v dalších devíti ztvárnila postavy různých sester.');
INSERT INTO movie VALUES (1318, 'MASH/MASH 192.mp4', 'Morální vítězství', 11, 1, 'cz', '  ', 'en', 'Moral Victory', 'http://www.csfd.cz/film/68850-mash/', 'MASH 192.jpg', 192, 'Plukovník Potter se naštve kvůli neustálým stížnostem Hawkeye a B.J. na kvalitu kulturního života ve 4077 a jmenuje je ''důstojníky přes morálku''. Těm se nakonec i přes hrozící fiasko podaří uspořádat večírek ve stylu pikniku na pláži a nálada v táboře je zachráněna. Mezitím má Charles starosti se svým pacientem, kterému sice zachránil nohu, ale jeho ruka zůstala necitlivá, a tak nemohl pokračovat ve své kariéře klavírního virtuosa. Charles mu sežene noty skladeb Maurice Ravela pro pianistu, který přišel o ruku v první světové válce.');
INSERT INTO movie VALUES (1319, 'MASH/MASH 193.mp4', 'Všichni dobří vojáci', 11, 1, 'cz', '  ', 'en', 'Old Soldiers', 'http://www.csfd.cz/film/68850-mash/', 'MASH 193.jpg', 193, 'Hawkeye je jmenován zástupcem velitele, když plukovník Potter odjíždí na tajemnou cestu do Tokia. Všichni se obávají o plukovníkův zdravotní stav, ale nakonec se vše vysvětlí. Hawkeye čelí problémům s velkou skupinou uprchlíků, z nichž hlavně děti potřebují lékařskou péči.');
INSERT INTO movie VALUES (1320, 'MASH/MASH 194.mp4', 'Pomocná ruka', 11, 1, 'cz', '  ', 'en', 'Lend a Hand', 'http://www.csfd.cz/film/68850-mash/', 'MASH 194.jpg', 194, 'V táboře zavládne nuda, a tak se B.J. rozhodne uspořádat narozeninovou párty pro Hawkeyho, přestože žádné narozeniny nemá. Hawkeye ale musí odjet na frontu pro zraněného doktora z první pomoci. Hawkeyho doprovází i dr. Borelli, se kterým se Hawkeye zpočátku nesnáší, ale nakonec musí spojit síly, aby vojáka společně zachránili. Pozn.: Dr. Borelliho, který se už v seriálu objevil (epizoda 66), hraje');
INSERT INTO movie VALUES (1321, 'MASH/MASH 195.mp4', 'Sbohem, krutý světe', 11, 1, 'cz', '  ', 'en', 'Goodbye, Cruel World', 'http://www.csfd.cz/film/68850-mash/', 'MASH 195.jpg', 195, 'Klinger si vyzdobí svou pracovnu po svém - v orientálním stylu, ale setká se s nepochopením, a proto se rozhodne utéct, ale nakonec si vše rozmyslí. Mezitím je přivezen do 4077 americko-korejsky válečný hrdina, který se pokusí o sebevraždu, protože má být propuštěn z armády a jet domů. Musí být k němu povolán doktor Freedman.');
INSERT INTO movie VALUES (1322, 'MASH/MASH 196.mp4', 'Sezóna hlouposti', 11, 1, 'cz', '  ', 'en', 'April Fools', 'http://www.csfd.cz/film/68850-mash/', 'MASH 196.jpg', 196, 'Ve 4077 se rozpoutá bouře aprílových žertíku, ale je ohlášena návštěva plukovníka Tuckera, který je známý tvrďák. Plukovník Potter se snaží všem žertíkům zamezit, což se mu ale nepovede. Nakonec vyjde najevo, že plukovník Tucker je Potterův přítel, který tak vlastně nachytal všechny. Pozn.: Od této epizody se pravidelně mezi osazenstvem tábora objevuje vykutálený seržant Luther Rizzo. Jeho představitel G.W. Bailey se objevil už v epizodě Bitva o kapitána, kde sehrál epizodní roli opilého vojáka.');
INSERT INTO movie VALUES (1323, 'MASH/MASH 197.mp4', 'Válečná dopisovatelka', 11, 1, 'cz', '  ', 'en', 'War Co-Respondent', 'http://www.csfd.cz/film/68850-mash/', 'MASH 197.jpg', 197, 'B.J. velmi znejistí, když se sblíží s mladou válečnou dopisovatelkou Aggie O´Sheaovou, která se do něho zamiluje.');
INSERT INTO movie VALUES (1324, 'MASH/MASH 198.mp4', 'Cena války', 11, 1, 'cz', '  ', 'en', 'Back Pay', 'http://www.csfd.cz/film/68850-mash/', 'MASH 198.jpg', 198, 'Hawkeye se dozví, že civilní doktoři ve Státech vydělávají na válce velké peníze, a rozhodne se požadovat na armádě peníze za své doktorské služby. Mezitím Charles demonstruje americké lékařské schopnosti třem korejským medikům.');
INSERT INTO movie VALUES (1325, 'MASH/MASH 199.mp4', 'Betonové přátelství', 11, 1, 'cz', '  ', 'en', 'Cementing Relationships', 'http://www.csfd.cz/film/68850-mash/', 'MASH 199.jpg', 199, 'Už několik pacientů se těsně po operaci infikovalo. Důvodem je nehygienická dřevěná podlaha v operačním stanu. Lékaři tak dlouho žádají o betonový povrch, až se rozhodnou jej vybudovat sami. Díky Klingerovi se podaří sehnat cement, jako organizátor však Klinger udělá drobnou chybičku, díky které se namísto betonu na podlaze objeví cementová kaše. Nakonec se však vše zvládne, včetně Margaretina problému se zamilovaným italským poručíkem. Ruku k dílu je donucen přiložit i Charles.');
INSERT INTO movie VALUES (1326, 'MASH/MASH 200.mp4', 'Žádné pocení', 11, 1, 'cz', '  ', 'en', 'No Sweat', 'http://www.csfd.cz/film/68850-mash/', 'MASH 200.jpg', 200, 'Subtropické teploty sužují obyvatele tábora. Margaretě se navíc na intimním místě udělá nepříjemný bolák. Protože není jedinou, kdo takto trpí, rychle dojde potřebný lék. Když plukovník Potter objednává novou dodávku, někdo zapne táborový rozhlas, takže se o Margaretiných problémech dozví opravdu všichni. Šťabajzna se ale pomstí...');
INSERT INTO movie VALUES (1327, 'MASH/MASH 201.mp4', 'Dopisy', 11, 1, 'cz', '  ', 'en', 'Letters', 'http://www.csfd.cz/film/68850-mash/', 'MASH 201.jpg', 201, 'Obyvatelé MASHe plní neobvyklý úkol. Odpovídají na dopisy, které poslali vojákům na frontu desetiletí školáci.');
INSERT INTO movie VALUES (1328, 'MASH/MASH 202.mp4', 'Nejlepší nepřítel', 11, 1, 'cz', '  ', 'en', 'The Best of Enemies', 'http://www.csfd.cz/film/68850-mash/', 'MASH 202.jpg', 202, 'Hawkeye při cestě na dovolenou do Soulu narazí na severokorejského vojáka, který jej donutí, aby se postaral o těžce zraněného přítele. Zranění jsou však příliš vážná...');
INSERT INTO movie VALUES (1329, 'MASH/MASH 203.mp4', 'Den otců', 11, 1, 'cz', '  ', 'en', 'Father´s Day', 'http://www.csfd.cz/film/68850-mash/', 'MASH 203.jpg', 203, 'Jeden z pacientů, které se podařilo lékařům zachránit, pošle jako výraz vděku zvláštní dárek - půlku krávy. Toto nečekané obohacení jídelníčku se zalíbí všem, Margareta však má jiné starosti - do MASHe přijíždí na návštěvu její otec a doktoři se mu příliš nezamlouvají.');
INSERT INTO movie VALUES (1330, 'MASH/MASH 204.mp4', 'Verbování', 11, 1, 'cz', '  ', 'en', 'Your Retention Please', 'http://www.csfd.cz/film/68850-mash/', 'MASH 204.jpg', 204, 'Klinger dostal dopis od své dívky, v němž mu sděluje, že jej opouští a vdává se za jeho nejlepšího přítele. Zoufalý Max se opije a podepíše smlouvu na šestiletou službu v armádě. Ze závazku jej může dostat už jen plukovník Potter...');
INSERT INTO movie VALUES (1331, 'MASH/MASH 205.mp4', 'Hlavní cena', 11, 1, 'cz', '  ', 'en', 'Taking the Fifth', 'http://www.csfd.cz/film/68850-mash/', 'MASH 205.jpg', 205, 'Hawkeye dostane nápad uspořádat soutěž o rande s ním a s lahví vzácného francouzského vína. Vítězka ale přivede nečekané překvapení - celou ženskou část tábora. Klingerovi se podaří získat další lahve pro Charlese, ale nakonec je musí nalít do chladiče svého džípu.');
INSERT INTO movie VALUES (1332, 'MASH/MASH 206.mp4', 'Smrt na prázdninách', 11, 1, 'cz', '  ', 'en', 'Death Takes a Holiday', 'http://www.csfd.cz/film/68850-mash/', 'MASH 206.jpg', 206, 'Vánoce v Koreji - konvoj se zásobami je přepaden, proto se každý v táboře snaží sehnat nějakou náhradu, aby vánoční svátky stály za to - hlavně pro korejské sirotky, které na oslavy pozvali. Bez některých doktorů se ale děti musejí obejít - do nemocnice je dopraven voják s prostřelenou hlavou.');
INSERT INTO movie VALUES (1333, 'MASH/MASH 207.mp4', 'Válka pro všechny sezóny', 11, 1, 'cz', '  ', 'en', 'A War for all Seasons', 'http://www.csfd.cz/film/68850-mash/', 'MASH 207.jpg', 207, 'MASH potřebuje nutně nový přístroj pro dialýzu. V Koreji se ale dá těžko sehnat. Proto se věci ujímají Hawkeye a B.J. a objednají pomocí katalogů v USA potřebné součástky, ze kterých pak přístroj sestavují.');
INSERT INTO movie VALUES (1334, 'MASH/MASH 208.mp4', 'Řekněte to mariňákům', 11, 1, 'cz', '  ', 'en', 'Tell it to the Marines', 'http://www.csfd.cz/film/68850-mash/', 'MASH 208.jpg', 208, 'Do MASHe je dopraven mladý, lehce raněný voják, který bojuje na americké straně, ačkoli je holandského původu. Jeho matka, která se ve státech rozvedla, má být nyní deportována. Hawkeye a B.J. s pomocí tisku zorganizují kampaň, aby této nespravedlnosti zabránili.');
INSERT INTO movie VALUES (1335, 'MASH/MASH 209.mp4', 'Nepříjemné zprávy', 11, 1, 'cz', '  ', 'en', 'Depressing News', 'http://www.csfd.cz/film/68850-mash/', 'MASH 209.jpg', 209, 'Klasický příklad armádní organizace - do tábora přijde dodávka 500 000 špachtlí. Hawkeye z nich postaví věž a ve chvíli, kdy navštíví MASH reportér listu Hvězdy a pruhy ji odpálí trhavinou.');
INSERT INTO movie VALUES (1336, 'MASH/MASH 210.mp4', 'Operace přátelství', 11, 1, 'cz', '  ', 'en', 'Operation Friendship', 'http://www.csfd.cz/film/68850-mash/', 'MASH 210.jpg', 210, 'B.J. by měl být operován - je nepříjmeně zraněn na pravé ruce. Zlé je, že nikdo z lékařů v táboře není schopen zranění přesně diagnostikovat. Plukovník Potter povolává specialistu...');
INSERT INTO movie VALUES (1337, 'MASH/MASH 211.mp4', 'Vážná věc', 11, 1, 'cz', '  ', 'en', 'No Laughing Matter', 'http://www.csfd.cz/film/68850-mash/', 'MASH 211.jpg', 211, 'Charles Winchester může za své přeložení do MASHe děkovat jednomu důstojníkovi, sloužícímu v Tokiu. Právě on přijede do tábora na inspekci. Charles se chtěl nejdřív pomstít, ale pak doufá, že mu pomůže k návratu do Tokia. Jeho plány ale selžou.');
INSERT INTO movie VALUES (1338, 'MASH/MASH 212.mp4', 'Ó, to si tancujeme', 11, 1, 'cz', '  ', 'en', 'Oh, How We Danced', 'http://www.csfd.cz/film/68850-mash/', 'MASH 212.jpg', 212, 'Čím blíž je B.J.ovo výročí sňatku, tím je smutnější a ve větší depresi. Hawkeye mu chce pomoci a postará se o to, aby B.J.ova manželka natočila krátký film, kde je ona i jejich malá dcera.');
INSERT INTO movie VALUES (1339, 'MASH/MASH 213.mp4', 'Až do dna', 11, 1, 'cz', '  ', 'en', 'Bottom Up', 'http://www.csfd.cz/film/68850-mash/', 'MASH 213.jpg', 213, 'Margaretina nejlepší přítelkyně, také sestra v MASHi, má těžké problémy s alkoholem. Nechce si je ale přiznat, dokud během jedné operace málem nedojde ke katastrofě. Margareta musí jako šéfka zasáhnout...');
INSERT INTO movie VALUES (1340, 'MASH/MASH 214.mp4', 'Rudobílé blues', 11, 1, 'cz', '  ', 'en', 'The Red/White Blues', 'http://www.csfd.cz/film/68850-mash/', 'MASH 214.jpg', 214, 'Drobná nepozornost má překvapivé následky. Klinger objednal prostředek proti malárii, který ovšem u vojáků černé pleti vyvolává nežádoucí vedlejší účinky. Je za to plukovníkem kárán, ovšem záhy se ukáže, že podobné příznaky mají i bělošští vojáci.');
INSERT INTO movie VALUES (1341, 'MASH/MASH 215.mp4', 'Pozdrav pánbu, Hawkeye', 11, 1, 'cz', '  ', 'en', 'Bless You, Hawkeye', 'http://www.csfd.cz/film/68850-mash/', 'MASH 215.jpg', 215, 'Hawkeye začne kýchat a nikdo v MASHi netuší, proč. Po všemožném zkoumáním a různých terapiích nakonec pomůže ten, od koho se nejméně čekalo. Pychiatr.');
INSERT INTO movie VALUES (1342, 'MASH/MASH 216.mp4', 'Život, který jsi zachránil', 11, 1, 'cz', '  ', 'en', 'The Life You Save', 'http://www.csfd.cz/film/68850-mash/', 'MASH 216.jpg', 216, 'Náhlá smrt pacienta šokuje Charlese Winchestera. Začne intenzivně přemýšlet o smrti.');
INSERT INTO movie VALUES (1343, 'MASH/MASH 217.mp4', 'To je showbyznis 1', 11, 1, 'cz', '  ', 'en', 'That''s Show Biz 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 217.jpg', 217, 'Marina je členkou umělecké skupiny, která jezdí bavit vojáky na frontě. Náhle začne mít bolesti v břiše. Dostane se do MASHe, kde jí diagnostikují zápal slepého střeva. Kolegové ji navštíví a předvedou v táboře svou show. Klingera to přivede na nápad živit se jako komik. Zřejmě by ale zemřel hlady.');
INSERT INTO movie VALUES (1344, 'MASH/MASH 218.mp4', 'To je showbyznis 2', 11, 1, 'cz', '  ', 'en', 'That''s Show Biz 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 218.jpg', 218, 'Marina je členkou umělecké skupiny, která jezdí bavit vojáky na frontě. Náhle začne mít bolesti v břiše. Dostane se do MASHe, kde jí diagnostikují zápal slepého střeva. Kolegové ji navštíví a předvedou v táboře svou show. Klingera to přivede na nápad živit se jako komik. Zřejmě by ale zemřel hlady.');
INSERT INTO movie VALUES (1345, 'MASH/MASH 219.mp4', 'Pokrevní bratři', 11, 1, 'cz', '  ', 'en', 'Blood Brothers', 'http://www.csfd.cz/film/68850-mash/', 'MASH 219.jpg', 219, 'Do MASHe přivezou dva zraněné vojáky. Poté, co jeden chce druhému dát svou krev se zjistí, že ochotný dárce má leukémii...');
INSERT INTO movie VALUES (1346, 'MASH/MASH 220.mp4', 'Předpovězená sága', 11, 1, 'cz', '  ', 'en', 'The Foresight Saga', 'http://www.csfd.cz/film/68850-mash/', 'MASH 220.jpg', 220, 'Protože vesnice, v níž žije, je neustále pod palbou, uteče mladý Korejec Pak Sung do MASHe. Geniální pěstitel je se svou zeleninou v táboře velmi vítán. Ale Pak Sung nemůže zůstat. Ale protože Radar má na své farmě v Americe problémy, pošlou nakonec mladíka k němu jako pomocníka.');
INSERT INTO movie VALUES (1347, 'MASH/MASH 221.mp4', 'Krize identity', 11, 1, 'cz', '  ', 'en', 'Identity Crisis', 'http://www.csfd.cz/film/68850-mash/', 'MASH 221.jpg', 221, 'Nemocný voják se vydává za svého padlého kolegu. Další zraněný se pokouší připravit se na budoucí povolání agenta s nemovitostmi...');
INSERT INTO movie VALUES (1348, 'MASH/MASH 222.mp4', 'Zaručené informace', 11, 1, 'cz', '  ', 'en', 'Rumor at the Top', 'http://www.csfd.cz/film/68850-mash/', 'MASH 222.jpg', 222, 'Důležitý generál vzkázal do MASHe, že tábor navštíví. Kolují divoké spekulace, které vyvrcholí tvrzením, že tábor bude zrušen. Plukovník Potter musí zasáhnout...');
INSERT INTO movie VALUES (1349, 'MASH/MASH 223.mp4', 'Jen jim to nandej, Hawkeye', 11, 1, 'cz', '  ', 'en', 'Give ''Em Hell, Hawkeye', 'http://www.csfd.cz/film/68850-mash/', 'MASH 223.jpg', 223, 'MASH potřebuje nutně bojler. Ale odpovědný důstojník chce něco za něco - dodá přístroj jen tehdy, pokud bude tábor čertvě natřen. Hawkeye proto píše dopis prezidentovi Harrymu Trumanovi.');
INSERT INTO movie VALUES (1350, 'MASH/MASH 224.mp4', 'Pracující žena', 11, 1, 'cz', '  ', 'en', 'Wheelers and Dealers', 'http://www.csfd.cz/film/68850-mash/', 'MASH 224.jpg', 224, 'B.J. je zdrcený ze zpráv z domova. Jeho manželka musí jít pracovat, aby uživila rodinu. B.J. se rozčiluje, že nemůže být doma, aby ji hmotně zajistil...');
INSERT INTO movie VALUES (1351, 'MASH/MASH 225.mp4', 'Komunikační potíže', 11, 1, 'cz', '  ', 'en', 'Communication Breakdown', 'http://www.csfd.cz/film/68850-mash/', 'MASH 225.jpg', 225, 'Zraněný severokorejský voják Kim se nechce nechat ošetřit od Američanů. Po ošetření má být dopraven do jihokorejského zajetí. V eskortě, která na něj čeká, je jeho bratr. Mají zakázaný kontakt, ale Hawkeye a Margareta jim dokáží poradit...');
INSERT INTO movie VALUES (1607, '201212/Bolek a Lolek - pohadky - Zakliaty zamok.mp4', 'Zakliaty zamok', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Zakliaty zamok.jpg', 67, NULL);
INSERT INTO movie VALUES (1352, 'MASH/MASH 226.mp4', 'Krátký proces 1', 11, 1, 'cz', '  ', 'en', 'Snap Judgement 1', 'http://www.csfd.cz/film/68850-mash/', 'MASH 226.jpg', 226, 'MASH zažívá vlnu krádeží. Mimo jiné se ztratí i kamera, kterou poslal vděčný otec jednoho ze zachráněných pacientů. Klinger ji později objeví na trhu. Získá ji zpět, ale je zatčen a obviněn z krádeží. B.J. a Hawkeye vymyslí past a polapí skutečného zloděje.');
INSERT INTO movie VALUES (1353, 'MASH/MASH 227.mp4', 'Krátký proces 2', 11, 1, 'cz', '  ', 'en', 'Snap Judgement 2', 'http://www.csfd.cz/film/68850-mash/', 'MASH 227.jpg', 227, 'MASH zažívá vlnu krádeží. Mimo jiné se ztratí i kamera, kterou poslal vděčný otec jednoho ze zachráněných pacientů. Klinger ji později objeví na trhu. Získá ji zpět, ale je zatčen a obviněn z krádeží. B.J. a Hawkeye vymyslí past a polapí skutečného zloděje.');
INSERT INTO movie VALUES (1354, 'MASH/MASH 228.mp4', 'To byl den po Vánocích', 11, 1, 'cz', '  ', 'en', '''Twas the Day After Christmas', 'http://www.csfd.cz/film/68850-mash/', 'MASH 228.jpg', 228, 'V MASHi slaví Vánoce angličtí spojenci. 25. prosince si důstojníci a vojáci na den vymění svá místa. Klinger je velitel a Potter úředník. Nejhůře dopadl Charles - musí do kuchyně.');
INSERT INTO movie VALUES (1355, 'MASH/MASH 229.mp4', 'Smrtí nic nekončí', 11, 1, 'cz', '  ', 'en', 'Follies of the Living - Concerns of the Dead', 'http://www.csfd.cz/film/68850-mash/', 'MASH 229.jpg', 229, 'Pro mladého vojáka je každá lékařská pomoc marná - zemře na operačním stole. Duše opustí jeho tělo a pokouší se komunikovat se živými...');
INSERT INTO movie VALUES (1356, 'MASH/MASH 230.mp4', 'Narozeninové dívky', 11, 1, 'cz', '  ', 'en', 'The Birthday Girls', 'http://www.csfd.cz/film/68850-mash/', 'MASH 230.jpg', 230, 'MASH dostane úkol vyzkoušet při vhodné příležitosti nový lék. Ten se hodí ve chvíli, kdy se do tábora vydá korejský sedlák s krávou, která je březí, ale nebude schopná přivést své tele na svět vlastními silami.');
INSERT INTO movie VALUES (1357, 'MASH/MASH 231.mp4', 'Krev a střeva', 11, 1, 'cz', '  ', 'en', 'Blood and Guts', 'http://www.csfd.cz/film/68850-mash/', 'MASH 231.jpg', 231, 'Slavný válečný reportér Clayton Kibbee se objeví v MASHi a chce také zažít "trochu války". Protože je ne frontu dlouhá cesta, jede tam na B.J.ově motorce. Má však nehodu a těžce se zraní...');
INSERT INTO movie VALUES (1358, 'MASH/MASH 232.mp4', 'Mše svatá', 11, 1, 'cz', '  ', 'en', 'A Holy Mess', 'http://www.csfd.cz/film/68850-mash/', 'MASH 232.jpg', 232, 'Svobodník Nick Gillis se dozvěděl od svého kamaráda, že jeho manželka právě porodila, ačkoli on už je rok v Koreji. Rozhodne se dezertovat a jet domů. Do tábora přijíždí vojenská policie. Zasáhne však otec Mulcahy...');
INSERT INTO movie VALUES (1359, 'MASH/MASH 233.mp4', 'Zub tě osvobodí', 11, 1, 'cz', '  ', 'en', 'The Tooth Shall Set You Free', 'http://www.csfd.cz/film/68850-mash/', 'MASH 233.jpg', 233, 'Velitel jedné jednotky chce poslat všechny zraněné černošské vojáky domů. Když Hawkeye a B.J. pátrají po důvodu, zjistí, že jde o nenapravitelného rasistu. Těm se podaří pomocí malé lsti dostat velitele z armády pryč.');
INSERT INTO movie VALUES (1360, 'MASH/MASH 234.mp4', 'Tlakové body', 11, 1, 'cz', '  ', 'en', 'Pressure Points', 'http://www.csfd.cz/film/68850-mash/', 'MASH 234.jpg', 234, 'Plukovník Potter je zralý pro psychiatra. Najednou má strach vstoupit na operační sál. Ale Sidney Freeman mu dokáže pomoci...');
INSERT INTO movie VALUES (1361, 'MASH/MASH 235.mp4', 'Kde je vůle, tam je i válka', 11, 1, 'cz', '  ', 'en', 'Where There''s a Will, There''s a War', 'http://www.csfd.cz/film/68850-mash/', 'MASH 235.jpg', 235, 'Polní nemocnice na frontě potřebuje nutně chirurga. Řada je B.J.ovi,ale ten je na dovolené v Soulu. Na cestu se vydává Hawkeye. Když je nemocnice pod těžkou palbou, sepisuje svoji poslední vůli.');
INSERT INTO movie VALUES (1362, 'MASH/MASH 236.mp4', 'Všeobecné pozdvižení', 11, 1, 'cz', '  ', 'en', 'Promotion Commotion', 'http://www.csfd.cz/film/68850-mash/', 'MASH 236.jpg', 236, 'Jednou za rok dochází k povyšování. Zájemci se pokoušejí ovlivnit důstojníky - a uchylují se od vydírání přes násilí až ke korupci...');
INSERT INTO movie VALUES (1363, 'MASH/MASH 237.mp4', 'Hrdinové', 11, 1, 'cz', '  ', 'en', 'Heroes', 'http://www.csfd.cz/film/68850-mash/', 'MASH 237.jpg', 237, 'V rámci zábavného programu pro vojáky se do MASHe dostane světově proslulý boxer. Následují jej početní novináři. Boxer bohužel zemře v táboře na infarkt. Ještě než se zpráva dostane k novinářům, napadne Klingera zajímavý plán.');
INSERT INTO movie VALUES (1364, 'MASH/MASH 238.mp4', 'Synové a kuželkáři', 11, 1, 'cz', '  ', 'en', 'Sons and Bowlers', 'http://www.csfd.cz/film/68850-mash/', 'MASH 238.jpg', 238, 'Hawkeyho znepokojí zpráva, že jeho otec musí do nemocnice - myslí si, že je to něco vážného a rád by s otcem mluvil. V té době dorazí do tábora skupina námořníků, která tu soutěží se všemi ostatními. Protože jsou ve skvělé formě, ve všem vítězí. Potter chce vyhrát alespoň v kuželkách.');
INSERT INTO movie VALUES (1365, 'MASH/MASH 239.mp4', 'Zachyť to', 11, 1, 'cz', '  ', 'en', 'Picture This', 'http://www.csfd.cz/film/68850-mash/', 'MASH 239.jpg', 239, 'Lékaři v MASHi čelí každodennímu stresu. Někdy je situace nesnesitelná. Zvláště, když Charles ve dne v noci poslouchá vážnou hudbu a B.J. všechny obtěžuje s problémy své malé dcerky. Hawkeye se rozhodne odstěhovat z Bažiny.');
INSERT INTO movie VALUES (1366, 'MASH/MASH 240.mp4', 'Kdo ví?', 11, 1, 'cz', '  ', 'en', 'Who Knew?', 'http://www.csfd.cz/film/68850-mash/', 'MASH 240.jpg', 240, 'Náhle zemře mladá sestra, která byla s Hawkeym. Když si Hawkey připravuje smuteční řeč, uvědomí si, že ji vlastně doopravdy neznal. Otec Mulcahy najde v jejích věcech deník a díky této četbě pronese Hawkey úchvatný projev...');
INSERT INTO movie VALUES (1367, 'MASH/MASH 241.mp4', 'Zlořečená koza', 11, 1, 'cz', '  ', 'en', 'That Darn Kid', 'http://www.csfd.cz/film/68850-mash/', 'MASH 241.jpg', 241, 'Klinger získal na černém trhu kozu. A bohužel sežere 20 000 dolarů, které má pokladník Hawkeye na vyplacení žoldu. Vláda pošle zvláštního úředníka, aby záležitost prošetřil - a ten chce, aby Hawkeye peníze zaplatil ze svého.');
INSERT INTO movie VALUES (1368, 'MASH/MASH 242.mp4', 'Střízlivý Měsíc', 11, 1, 'cz', '  ', 'en', 'The Moon is Not Blue', 'http://www.csfd.cz/film/68850-mash/', 'MASH 242.jpg', 242, 'Při teplotách okolo 40° Celsia a po celodenní dřině touží člověk po jediném - po vychlazeném drinku. Mezi zraněnými je však dvouhvězdičkový generál, kterému se nelíbí konzumace alkoholu v táboře. Nařídí zavřít bar a chce zavést úplnou prohibici.');
INSERT INTO movie VALUES (1369, 'MASH/MASH 243.mp4', 'Koukni na mě', 11, 1, 'cz', '  ', 'en', 'Look Me Over', 'http://www.csfd.cz/film/68850-mash/', 'MASH 243.jpg', 243, 'Do MASHe dorazila nová zásilka desek a Juke-Box se ani na chvíli nezastaví. Všechny sestry jsou zadány až na Kellyovou. Ta se jednoho dne napije na kuráž a svěří Hawkeymu co k němu cítí.');
INSERT INTO movie VALUES (1370, 'MASH/MASH 244.mp4', 'Propaganda', 11, 1, 'cz', '  ', 'en', 'Foreign Affairs', 'http://www.csfd.cz/film/68850-mash/', 'MASH 244.jpg', 244, 'Důstojník pro styk s veřejností má přesvědčit severokorejského pilota, který omylem přistál na nepřátelském území, aby předstíral dezerci. Nabídne mu spoustu peněz, když bude tvrdit, že se svým MIGem letěl úmyslně do Jižní Koreje. Voják ale odmítne a jde radši do zajateckého tábora.');
INSERT INTO movie VALUES (1371, 'MASH/MASH 245.mp4', 'Pochybnosti', 11, 1, 'cz', '  ', 'en', 'Settling Debts', 'http://www.csfd.cz/film/68850-mash/', 'MASH 245.jpg', 245, 'Plukovník Potter najde v poště dopis od své manželky adresovaný Hawkeymu. To jej pořádně vyvede z míry. Neví totiž, že jeho žena mu tímto způsobem chce sdělit šťastnou novinu, kterou má Hawkey oznámit na večírku s překvapením.');
INSERT INTO movie VALUES (1608, '201212/Bolek a Lolek - pohadky - Zlata rybka.mp4', 'Zlata rybka', 3, 5, 'cz', NULL, 'pl', NULL, NULL, 'Bolek a Lolek - pohadky - Zlata rybka.jpg', 68, NULL);
INSERT INTO movie VALUES (1372, 'MASH/MASH 246.mp4', 'Divoký vtipálek', 11, 1, 'cz', '  ', 'en', 'The Joker is Wild', 'http://www.csfd.cz/film/68850-mash/', 'MASH 246.jpg', 246, 'Všichni stále vzpomínají na Trappera, jeho smysl pro humor, jeho bláznivé nápady. B.J. toho už má dost, rozhodne se, že překoná svého předchůdce a každému v táboře něco vyvede. Kupodivu právě Hawkey zůstane ušetřen a tak si o sobě začne myslet, že jeho prostě nikdo "nedostane"...');
INSERT INTO movie VALUES (1373, 'MASH/MASH 247.mp4', 'Iluze nebo terapie', 11, 1, 'cz', '  ', 'en', 'Trick or Treatment', 'http://www.csfd.cz/film/68850-mash/', 'MASH 247.jpg', 247, 'Na operačním sále se vykládají tajemné příběhy. Každý někdy zažil něco"nevysvětlitelného", nebo o tom slyšel vyprávět, jenom Winchesterovi jsou takové historky pro smích. Brzy však sám začne zažívat podivné věci.');
INSERT INTO movie VALUES (1374, 'MASH/MASH 248.mp4', 'Senzační zpráva', 11, 1, 'cz', '  ', 'en', 'Bombshells', 'http://www.csfd.cz/film/68850-mash/', 'MASH 248.jpg', 248, 'Začne to jako nevinný vtip. Do světa je vypuštěna fáma a fámy se šíří rychlostí blesku. Najednou všichni včetně hlavního velení v Soulu chystají v MASHi 4077 slavnostní uvítání. Samotná Marilyn Monroe má přijet pozdravit statečné chlapce na frontě.');
INSERT INTO movie VALUES (1375, 'MASH/MASH 249.mp4', 'Běh pro peníze', 11, 1, 'cz', '  ', 'en', 'Run for the Money', 'http://www.csfd.cz/film/68850-mash/', 'MASH 249.jpg', 249, 'Běžeckého závodu roku v sousedním táboře se účastní vrcholový sportovec. A protože do 4077 má dorazit profesionální sprinter, ihned jej přihlásí do závodu. Bohužel domnělý sportovec je starý muž, otec sprintera. Sázky jsou uzavřeny a závod se musí uskutečnit. A tak za MASH 4077 musí nastoupit - s boží pomocí- otec Mulcahy.');
INSERT INTO movie VALUES (1376, 'MASH/MASH 250.mp4', 'OSN, noc a hudba', 11, 1, 'cz', '  ', 'en', 'U.N., the Night and the Music', 'http://www.csfd.cz/film/68850-mash/', 'MASH 250.jpg', 250, 'Do MASHe stále přijíždějí návštěvy ze spojeneckých armád. Během jedné takové návštěvy se Charles seznámí s Angličanem Randolphem. Charles je prostě okouzlen - Randolph má skvělé vychování, rozumí vážné hudbě a téměř ve všem jej předčí. Jeho okouzlení však pomine, když se ukáže, že Angličan je jen syn komorníka.');
INSERT INTO movie VALUES (1377, 'MASH/MASH 251.mp4', 'Podivní spolunocležníci', 11, 1, 'cz', '  ', 'en', 'Strange Bedfellows', 'http://www.csfd.cz/film/68850-mash/', 'MASH 251.jpg', 251, 'Potterův švagr je obchodně týden v Soulu a využije příležitosti, aby navštívil tchána. Náhodou se Potter dozví, že manžel jeho dcery měl na hotelu dámskou návštěvu. Rozhodne se zasáhnout a promluvit potterovským způsobem zeťovi do duše.');
INSERT INTO movie VALUES (1378, 'MASH/MASH 252.mp4', 'Neříkej už ne', 11, 1, 'cz', '  ', 'en', 'Say No More', 'http://www.csfd.cz/film/68850-mash/', 'MASH 252.jpg', 252, 'Mladý důstojník Collins se dostane se zraněním do tábora. Nejde však o nějakou závažnou záležitost, bohužel nastanou komplikace. Collins zemře a jeho otec, generál, se těžko smiřuje se synovou smrtí - obviňuje lékaře.');
INSERT INTO movie VALUES (1379, 'MASH/MASH 253.mp4', 'Přátelé a nepřátelé', 11, 1, 'cz', '  ', 'en', 'Friends and Enemies', 'http://www.csfd.cz/film/68850-mash/', 'MASH 253.jpg', 253, 'Vinou vojáka z povolání padlo mnoho jeho podřízených. Jde navíc o nejlepšího přítele plukovníka Pottera. Ten hodlá zasáhnout, i kdyby jej to mělo stát dlouholeté přátelství.');
INSERT INTO movie VALUES (1380, 'MASH/MASH 254.mp4', 'Dávej a ber', 11, 1, 'cz', '  ', 'en', 'Give and Take', 'http://www.csfd.cz/film/68850-mash/', 'MASH 254.jpg', 254, 'Nevděčný úkol, vybírat dobrovolné příspěvky na různé charitativní účely, má na starosti každý měsíc jiný důstojník. Protože většina nemá, co by dala, dostává se výběrčímu většinou jen slibů - posledním v řadě je Charles Winchester, který nakonec pořádně sáhne do peněženky.');
INSERT INTO movie VALUES (1381, 'MASH/MASH 255.mp4', 'Jak jde čas', 11, 1, 'cz', '  ', 'en', 'As Time Goes', 'http://www.csfd.cz/film/68850-mash/', 'MASH 255.jpg', 255, 'Na základě článku v novinách napadne Margaretu vytvořit takzvané časové pouzdro, do nějž by na paměť budoucím generacím dali příslušníci MASHe symboly své práce a svého pobytu v Koreji.');
INSERT INTO movie VALUES (1382, 'MASH/MASH 256 - Nashledanou sbohem a amen.mp4', 'Sbohem, na rozloučenou a amen', 11, 1, 'cz', '  ', 'en', 'Goodbye, Farewell and Amen', 'http://www.csfd.cz/film/68850-mash/', 'MASH 256 - Nashledanou sbohem a amen.jpg', 256, 'Poslední, dvouhodinový speciální díl zachycuje konec nemocnice MASH. Klinger se v něm ožení s korejskou dívkou, Hawkeye přetrpí nejtěžší psychickou krizi svého válečného života a nakonec se všichni vracejí domů. Válka skončila.');
INSERT INTO movie VALUES (1383, 'Matrix/Matrix Reloaded.m4v', 'Matrix Reloaded', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0234215/', 'Matrix Reloaded.jpg', 2, 'Directed by Andy Wachowski, Lana Wachowski. With Keanu Reeves, Laurence Fishburne, Carrie-Anne Moss, Hugo Weaving. Neo and the rebel leaders estimate that they have 72 hours until 250,000 probes discover Zion and destroy it and its inhabitants. During this, Neo must decide how he can save Trinity from a dark fate in his dreams.');
INSERT INTO movie VALUES (1384, 'Matrix/Matrix Revolutions.m4v', 'Matrix Revolutions', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0242653/', 'Matrix Revolutions.jpg', 3, 'Directed by Andy Wachowski, Lana Wachowski. With Keanu Reeves, Laurence Fishburne, Carrie-Anne Moss, Hugo Weaving. The human city of Zion defends itself against the massive invasion of the machines as Neo fights to end the war at another front while also opposing the rogue Agent Smith.');
INSERT INTO movie VALUES (1385, 'Matrix/Matrix.m4v', 'Matrix', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0133093/', 'Matrix.jpg', 1, 'Directed by Andy Wachowski, Lana Wachowski. With Keanu Reeves, Laurence Fishburne, Carrie-Anne Moss, Hugo Weaving. A computer hacker learns from mysterious rebels about the true nature of his reality and his role in the war against its controllers.');
INSERT INTO movie VALUES (1386, 'PrirodopisneX/Kozate devky - Titty Hookers.m4v', 'Kozate devky - Titty Hookers', 17, NULL, 'en', '  ', 'en', NULL, NULL, 'adult.png', 2, NULL);
