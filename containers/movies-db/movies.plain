INSERT INTO movie VALUES (2243, '201307/Spirit hrebec Cimmaronu Mustang CZ.m4v', 'Spirit Mustang', 3, 0, 'cz', NULL, 'en', 'Spirit: Stallion of the Cimarron', 'http://www.csfd.cz/film/30979-spirit-divoky-hrebec/', 'Spirit hrebec Cimmaronu Mustang CZ.jpg', NULL, 'Totéž studio, které vyrábělo "Shreka", se podílelo na animovaném dobrodružství o mladém mustangovi, který se snaží zachránit svůj domov a znovu získat ztracenou svobodu. Mustang na své odvážné a napínavé cestě divočinou amerického pohraničí prožívá nezapomenutelné přátelství s mladým a hrdinným indiánem z kmene Lakota, vyzraje nad neúnavnou eskadrou vojáků a zamiluje se do krásné klisny jménem Rain.<span class="source">(oficiální text distributora)</span>');
