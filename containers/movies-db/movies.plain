INSERT INTO movie VALUES (910, 'Animatrix/Animatrix 1 - Final Flight of the Osiris.m4v', 'Final Flight of the Osiris', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0350934/', 'Animatrix 1 - Final Flight of the Osiris.jpg', 4, 'Directed by Andrew R. Jones. With Kevin Michael Richardson, Pamela Adlon, John Di Maggio, Tom Kenny. After discovering a force of machines on its way to Zion, the crew of the hovercraft Osiris attempts to warn the city by using the Matrix.');
INSERT INTO movie VALUES (911, 'Animatrix/Animatrix 2 - The Second Renaissance Part 1.m4v', 'The Second Renaissance Part 1', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0366179/', 'Animatrix 2 - The Second Renaissance Part 1.jpg', 5, 'Directed by Mahiro Maeda. With Dane A. Davis, Debi Derryberry, Julia Fletcher, Dwight Schultz. A tour is taken into the Zion archives, where the history of the real world and the rise of the machines is shown to viewers.');
INSERT INTO movie VALUES (912, 'Animatrix/Animatrix 3 - The Second Renaissance Part 2.m4v', 'The Second Renaissance Part 2', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0368575/', 'Animatrix 3 - The Second Renaissance Part 2.jpg', 6, 'Directed by Mahiro Maeda. With Julia Fletcher, Dane A. Davis, Debi Derryberry, Jill Talley. With the robot city isolated and it&#x27;s ambassadors ejected form the United Nations, a trade war begins to protect the human economy from superior products...');
INSERT INTO movie VALUES (913, 'Animatrix/Animatrix 4 - Kid''s Story.m4v', 'Kid''s Story', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0368574/', 'Animatrix 4 - Kid''s Story.jpg', 7, 'Directed by Shinichir&#xF4; Watanabe. With Clayton Watson, Keanu Reeves, Carrie-Anne Moss, John DeMita. A teenager&#x27;s dreams which conclude with him falling off a building, lead him to believe that the reality he lives in, may not be so real after all.');
INSERT INTO movie VALUES (914, 'Animatrix/Animatrix 5 - Program.m4v', 'Program', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0366178/', 'Animatrix 5 - Program.jpg', 8, 'Directed by Yoshiaki Kawajiri. With Hedy Burress, Phil LaMarr, John Di Maggio. Cis and Duo, a pair of freed minds, practice their skills in an old-fashioned samurai sword-fight. Duo tells Cis that he&#x27;s tired of living outside the Matrix and he wants her to come with him... and he&#x27;s not accepting no for an answer.');
INSERT INTO movie VALUES (915, 'Animatrix/Animatrix 6 - World Record.m4v', 'World Record', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0368576/', 'Animatrix 6 - World Record.jpg', 9, 'Directed by Takeshi Koike. With Victor Williams, John Wesley, Alex Fernandez, Allison Smith. An athlete, Dan, breaks out of the matrix through a feat of strength and gets a brief glimse of the real world outside his existance.');
INSERT INTO movie VALUES (916, 'Animatrix/Animatrix 7 - Beyond.m4v', 'Beyond', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0372764/', 'Animatrix 7 - Beyond.jpg', 10, 'Directed by K&#xF4;ji Morimoto. With Hedy Burress, Tress MacNeille, Kath Soucie, Pamela Adlon. A young girl and a group of children discover that an abandoned house contains a wonderous nature to it');
INSERT INTO movie VALUES (917, 'Animatrix/Animatrix 8 - A Detective Story.m4v', 'A Detective Story', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0366177/', 'Animatrix 8 - A Detective Story.jpg', 11, 'Directed by Shinichir&#xF4; Watanabe. With James Arnold Taylor, Carrie-Anne Moss, Terrence &#x27;T.C.&#x27; Carson, Matt McKenzie. A private detective is hired to track down the computer hacker known as Trinity. But as he gets close his goal, reality takes a weird turn for him.');
INSERT INTO movie VALUES (918, 'Animatrix/Animatrix 9 - Matriculated.m4v', 'Matriculated', 4, 17, 'en', 'cz', 'en', NULL, 'http://www.imdb.com/title/tt0372765/', 'Animatrix 9 - Matriculated.jpg', 12, 'Directed by Peter Chung. With Melinda Clarke, Dwight Schultz, Rodney Saulsberry, James Arnold Taylor. Using the Matrix, a resistance group attempts to recruit a sentinel to their cause.');
