INSERT INTO movie VALUES (1082, 'James Bond/1965 James Bond - Thunderball.m4v', 'Thunderball', 15, 15, 'cz', '  ', 'en', 'Thunderball', 'http://www.imdb.com/title/tt0059800/', '1965 James Bond - Thunderball.jpg', 4, 'Directed by Terence Young. With Sean Connery, Claudine Auger, Adolfo Celi, Luciana Paluzzi. James Bond heads to The Bahamas to recover two nuclear warheads stolen by SPECTRE agent Emilio Largo in an international extortion scheme.');
INSERT INTO movie VALUES (1083, 'James Bond/1967 James Bond - Zijes len dvakrat -- You only live twice.m4v', 'Zijes len dvakrat', 15, 15, 'cz', '  ', 'en', 'You only live twice', 'http://www.imdb.com/title/tt0062512/', '1967 James Bond - Zijes len dvakrat -- You only live twice.jpg', 5, 'Directed by Lewis Gilbert. With Sean Connery, Akiko Wakabayashi, Mie Hama, Tetsur&#xF4; Tanba. Agent 007 and the Japanese secret service ninja force must find and stop the true culprit of a series of spacejackings before nuclear war is provoked.');
INSERT INTO movie VALUES (1084, 'James Bond/1969 James Bond - V tajnej sluzbe jej velicenstva -- On Her Majesty''s Secret Service.m4v', 'V tajnej sluzbe jej velicenstva', 15, 15, 'cz', '  ', 'en', 'On Her Majesty''s Secret Service', 'http://www.imdb.com/title/tt0064757/', '1969 James Bond - V tajnej sluzbe jej velicenstva -- On Her Majesty''s Secret Service.jpg', 6, 'Directed by Peter R. Hunt. With George Lazenby, Diana Rigg, Telly Savalas, Gabriele Ferzetti. James Bond woos a mob boss&#x27;s daughter and goes undercover to uncover the true reason for Blofeld&#x27;s allergy research in the Swiss Alps that involves beautiful women from around the world.');
INSERT INTO movie VALUES (1085, 'James Bond/1971 James Bond - Diamanty su vecne -- Diamonds Are Forever.m4v', 'Diamanty su vecne', 15, 15, 'cz', '  ', 'en', 'Diamonds Are Forever', 'http://www.imdb.com/title/tt0066995/', '1971 James Bond - Diamanty su vecne -- Diamonds Are Forever.jpg', 7, 'Directed by Guy Hamilton. With Sean Connery, Jill St. John, Charles Gray, Lana Wood. A diamond smuggling investigation leads James Bond to Las Vegas, where he uncovers an extortion plot headed by his nemesis, Ernst Stavro Blofeld.');
INSERT INTO movie VALUES (1086, 'James Bond/1973 James Bond - Zit a nechat zomriet -- Live and Let Die.m4v', 'Zit a nechat zomriet', 15, 15, 'cz', '  ', 'en', 'Live and Let Die', 'http://www.imdb.com/title/tt0070328/', '1973 James Bond - Zit a nechat zomriet -- Live and Let Die.jpg', 8, 'Directed by Guy Hamilton. With Roger Moore, Yaphet Kotto, Jane Seymour, Clifton James. 007 is sent to stop a diabolically brilliant heroin magnate armed with a complex organization and a reliable psychic tarot card reader.');
INSERT INTO movie VALUES (1087, 'James Bond/1974 James Bond - Muz so zlatou zbranou -- The Man with the Golden Gun.m4v', 'Muz so zlatou zbranou', 15, 15, 'cz', '  ', 'en', 'The Man with the Golden Gun', 'http://www.imdb.com/title/tt0071807/', '1974 James Bond - Muz so zlatou zbranou -- The Man with the Golden Gun.jpg', 9, 'Directed by Guy Hamilton. With Roger Moore, Christopher Lee, Britt Ekland, Maud Adams. Bond is led to believe that he is targeted by the world&#x27;s most expensive assassin and must hunt him down to stop him.');
INSERT INTO movie VALUES (1088, 'James Bond/1977 James Bond - Agent, ktory ma miloval -- The Spy Who Loved Me.m4v', 'Agent, ktory ma miloval', 15, 15, 'cz', '  ', 'en', 'The Spy Who Loved Me', 'http://www.imdb.com/title/tt0076752/', '1977 James Bond - Agent, ktory ma miloval -- The Spy Who Loved Me.jpg', 10, 'Directed by Lewis Gilbert. With Roger Moore, Barbara Bach, Curd J&#xFC;rgens, Richard Kiel. James Bond investigates the hijacking of British and Russian submarines carrying nuclear warheads with the help of a KGB agent whose lover he killed.');
INSERT INTO movie VALUES (1089, 'James Bond/1979 James Bond - Moonraker.m4v', 'Moonraker', 15, 15, 'cz', '  ', 'en', 'Moonraker', 'http://www.imdb.com/title/tt0079574/', '1979 James Bond - Moonraker.jpg', 11, 'Directed by Lewis Gilbert. With Roger Moore, Lois Chiles, Michael Lonsdale, Richard Kiel. James Bond investigates the mid-air theft of a space shuttle and discovers a plot to commit global genocide.');
INSERT INTO movie VALUES (1090, 'James Bond/1981 James Bond - Len pre tvoje oci -- For Your Eyes Only.m4v', 'Len pre tvoje oci', 15, 15, 'cz', '  ', 'en', 'For Your Eyes Only', 'http://www.imdb.com/title/tt0082398/', '1981 James Bond - Len pre tvoje oci -- For Your Eyes Only.jpg', 12, 'Directed by John Glen. With Roger Moore, Carole Bouquet, Topol, Lynn-Holly Johnson. Agent 007 is assigned to hunt for a lost British encryption device and prevent it from falling into enemy hands.');
INSERT INTO movie VALUES (1091, 'James Bond/1983 James Bond - Chobotnicka -- Octopussy.m4v', 'Chobotnicka', 15, 15, 'cz', '  ', 'en', 'Octopussy', 'http://www.imdb.com/title/tt0086034/', '1983 James Bond - Chobotnicka -- Octopussy.jpg', 13, 'Directed by John Glen. With Roger Moore, Maud Adams, Louis Jourdan, Kristina Wayborn. A fake Faberg&#xE9; egg and a fellow agent&#x27;s death leads James Bond to uncovering an international jewel smuggling operation, headed by the mysterious Octopussy, being used to disguise a nuclear attack on NATO forces.');
INSERT INTO movie VALUES (1092, 'James Bond/1985 James Bond - Vyhliadka na smrt -- A View to a Kill.m4v', 'Vyhliadka na smrt', 15, 15, 'cz', '  ', 'en', 'A View to a Kill', 'http://www.imdb.com/title/tt0090264/', '1985 James Bond - Vyhliadka na smrt -- A View to a Kill.jpg', 14, 'Directed by John Glen. With Roger Moore, Christopher Walken, Tanya Roberts, Grace Jones. An investigation of a horse-racing scam leads 007 to a mad industrialist who plans to create a worldwide microchip monopoly by destroying California&#x27;s Silicon Valley.');
INSERT INTO movie VALUES (1093, 'James Bond/1987 James Bond - Dych zivota -- The Living Daylights.m4v', 'Dych zivota', 15, 15, 'cz', '  ', 'en', 'The Living Daylights', 'http://www.imdb.com/title/tt0093428/', '1987 James Bond - Dych zivota -- The Living Daylights.jpg', 15, 'Directed by John Glen. With Timothy Dalton, Maryam d&#x27;Abo, Jeroen Krabb&#xE9;, Joe Don Baker. James Bond is living on the edge to stop an evil arms dealer from starting another world war. Bond crosses all seven continents in order to stop the evil Whitaker and General Koskov.');
INSERT INTO movie VALUES (1094, 'James Bond/1989 James Bond - Povolenie zabijat -- Licence to Kill.m4v', 'Povolenie zabijat', 15, 15, 'cz', '  ', 'en', 'Licence to Kill', 'http://www.imdb.com/title/tt0097742/', '1989 James Bond - Povolenie zabijat -- Licence to Kill.jpg', 16, 'Directed by John Glen. With Timothy Dalton, Robert Davi, Carey Lowell, Talisa Soto. James Bond leaves Her Majesty&#x27;s Secret Service to stop an evil drug lord and avenge his best friend, Felix Leiter.');
INSERT INTO movie VALUES (1095, 'James Bond/1995 James Bond - Zlate oko -- GoldenEye.m4v', 'Zlate oko', 15, 15, 'cz', '  ', 'en', 'GoldenEye', 'http://www.imdb.com/title/tt0113189/', '1995 James Bond - Zlate oko -- GoldenEye.jpg', 17, 'Directed by Martin Campbell. With Pierce Brosnan, Sean Bean, Izabella Scorupco, Famke Janssen. James Bond teams up with the lone survivor of a destroyed Russian research center to stop the hijacking of a nuclear space weapon by a fellow agent believed to be dead.');
INSERT INTO movie VALUES (1096, 'James Bond/1997 James Bond - Zajtrajsok nikdy neumiera -- Tomorrow Never Dies.m4v', 'Zajtrajsok nikdy neumiera', 15, 15, 'cz', '  ', 'en', 'Tomorrow Never Dies', 'http://www.imdb.com/title/tt0120347/', '1997 James Bond - Zajtrajsok nikdy neumiera -- Tomorrow Never Dies.jpg', 18, 'Directed by Roger Spottiswoode. With Pierce Brosnan, Jonathan Pryce, Michelle Yeoh, Teri Hatcher. James Bond heads to stop a media mogul&#x27;s plan to induce war between China and the UK in order to obtain exclusive global media coverage.');
INSERT INTO movie VALUES (1097, 'James Bond/1999 James Bond - Jeden svet nestace -- The World Is Not Enough.m4v', 'Jeden svet nestaci', 15, 15, 'cz', '  ', 'en', 'The World Is Not Enough', 'http://www.imdb.com/title/tt0143145/', '1999 James Bond - Jeden svet nestace -- The World Is Not Enough.jpg', 19, 'Directed by Michael Apted. With Pierce Brosnan, Sophie Marceau, Robert Carlyle, Denise Richards. James Bond uncovers a nuclear plot when he protects an oil heiress from her former kidnapper, an international terrorist who can&#x27;t feel pain.');
INSERT INTO movie VALUES (1098, 'James Bond/2002 James Bond - Dnes neumieraj-- Die Another Day.m4v', 'Dnes neumieraj', 15, 15, 'cz', '  ', 'en', 'Die Another Day', 'http://www.imdb.com/title/tt0246460/', '2002 James Bond - Dnes neumieraj-- Die Another Day.jpg', 20, 'Directed by Lee Tamahori. With Pierce Brosnan, Halle Berry, Rosamund Pike, Toby Stephens. James Bond is sent to investigate the connection between a North Korean terrorist and a diamond mogul who is funding the development of an international space weapon.');
